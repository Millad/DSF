          END;                                                                  
                                                                                
                                                                                
        WHEN ( 'GRBLFORS' )                                                     
          /* **************************************************** */            
          /*  FÅTT INN :    G R B L F O R S                       */            
          /* **************************************************** */            
          DO;                                                                   
            SUBSTR (ADDR                (GRBLFORS)                              
                ->  W01_HJELPE_IO,1,STG (GRBLFORS)) =  W01_IO;                  
            CALL    P010_LES_SEGM;                                              
          END;                                                                  
                                                                                
                                                                                
        WHEN ( 'GRUNNBIF' )                                                     
          /* **************************************************** */            
          /*  FÅTT INN :    G R B L F O R S                       */            
