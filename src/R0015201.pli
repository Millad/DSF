    /* ************************************************************** */        
                                                                                
                                                                                
                                                                                
                                                                                
    ALLOCATE  TRANS_SEGM_OMR;                                                   
    TRANS_SEGM_OMR                          =   ''                     ;        
    TRANS_OPPL_OMR                          =   ''                     ;        
    DB_STATUS_KODE     =   '  '       ;                                         
    IF FEIL_MELD_NR    =   0  THEN                                              
       PROGRAM_ID         =   'R0015201' ;                                      
    UIBPTR                =    UIB_PEKER ;                                      
                                                                                
    IF SUBSTR(CICS_NAVN,1,4) = 'HELP'     THEN                                  
             SSA1_TRANHIST_CMD.REL_OP_1 = ' =' ;                                
                                                                                
                                                                                
    IF   SEARCH_FNR                         =    0                 THEN         
      DO;                                                                       
         /* ********************************************** */                   
         /* NULL I FNR . KJØRING AVBRYTES                  */                   
         /* ********************************************** */                   
  L100:                                                                         
         FEIL_MELD_NR                 =   498;                                  
         FEIL_VED_LABEL               =  '100';                                 
         GO TO L999;                                                            
      END;                                                                      
                                                                                
    DO   J=1 TO 14  WHILE   ( W01.PERSON_IKKE_FUNNET )  ;                       
                                                                                
      /* ********************************************** */                      
      /* SJEKKER OM PESONEN ER LEST INN I  B01/B02 FØR. */                      
      /* ********************************************** */                      
                                                                                
      IF  B01.RF0PERSN.FNR (J)         =    SEARCH_FNR     THEN                 
        DO;                                                                     
          W01.NAVN                     =    B01.RF0PERSN.NAVN (J);              
          W01.PERSON_IKKE_FUNNET       =   '0'B;                                
        END;                                                                    
      ELSE                                                                      
      IF  B02.RF0PERSN.FNR (J)         =    SEARCH_FNR     THEN                 
        DO;                                                                     
          W01.NAVN                     =    B02.RF0PERSN.NAVN (J);              
          W01.PERSON_IKKE_FUNNET       =   '0'B;                                
        END;                                                                    
   END;                                                                         
                                                                                
    /* ********************************************************** */            
    /* SETTER INN VERDIENE I SSA'ENE.                             */            
    /* ********************************************************** */            
                                                                                
    SSA1_RF0PERSN.PKEY              =   SEARCH_FNR                 ;            
                                                                                
                                                                                
    SELECT  ( STATUS_TYPE );                                                    
                                                                                
       WHEN ('C'          )                                                     
            SSA1_TRANHIST.VTP       =   0                          ;            
                                                                                
       WHEN ('D'          )                                                     
                                                                                
         /* ***************************************************** */            
         /* I ØNSKET_STATUS LIGGER IFOMASJON OM HVILKEN           */            
         /* RELASJON-OPERATOR SOM SKAL BENYTTES .                 */            
         /* ***************************************************** */            
         DO;                                                                    
            SSA1_TRANHIST.REL_OP_2 =  ØNSKET_STATUS                ;            
            SSA1_TRANHIST.VTP      =  B01.VIRK_DATO_ÅMD (SØKER_IND) ;           
         END;                                                                   
                                                                                
     /* WHEN ('F' , 'Z'   ) */                                                  
       WHEN ('F'          )                                                     
                                                                                
            /* ************************************************** */            
            /* DENNE TYPE FÅS BARE VED FJERNING AV SISTE STATUS   */            
            /* BRUKER VIRKDATOEN I : DIV_PARAM_OMR                */            
            /* ************************************************** */            
                                                                                
            SSA1_TRANHIST.VTP   =  DIV_PARAM_OMR.TRANS_VIRKDATO_ÅMD ;           
                                                                                
       OTHERWISE                                                                
            SSA1_TRANHIST.VTP      =  B01.VIRK_DATO_ÅMD (SØKER_IND) ;           
                                                                                
    END;  /*  SELECT  ( STATUS_TYPE )                             */            
                                                                                
                                                                                
    /* ********************************************************** */            
    /*  SJEKKER OM DET ER FØRSTE TRANS SOM ØNSKES LEST            */            
    /*  HVIS ENDRING OG PERSONEN IKKE ER LEST INN FØR SPERRES HAN */            
    /*  GJENNOM    CALL TIL R0015601 .                            */            
    /*  ELLERS GJØRES ET VANLIG KALL (GU) MOT RF0PERSN .          */            
    /* ********************************************************** */            
                                                                                
    IF DIV_PARAM_OMR.FØRSTE_CALL_LES_TRANS                     THEN             
       DO;                                                                      
          IF STYRINGS_OMR.FUNKSJONSKODE  =    'E'   &                           
             STYRINGS_OMR.STYREKODE      =    'AT'  &                           
             W01.PERSON_IKKE_FUNNET                            THEN             
             DO;                                                                
                 POS_I_B01               =     14   ;                           
                 EXEC CICS LINK PROGRAM ('R0015601') COMMAREA (KOM_OMR);        
                                                                                
                 IF FEIL_MELD_NR         >     0               THEN             
                    GO   TO   L999;                                             
                                                                                
                 IF B01.FNR (POS_I_B01)     >     0         THEN                
                    DO;                                                         
                       W01.PERSON_IKKE_FUNNET  =    '0'B;                       
                       W01.NAVN   =  B01.RF0PERSN (POS_I_B01).NAVN;             
                    END;                                                        
                 ELSE                                                           
                    MER_DATA                =    '0'B;                          
             END;                                                               
          ELSE                                                                  
             CALL P005_LES_ROT_GU;                                              
       END;                                                                     
    ELSE                                                                        
       DO;                                                                      
          IF STYRINGS_OMR.FUNKSJONSKODE  =    'F'    !                          
            (STYRINGS_OMR.FUNKSJONSKODE  =    'E'    &                          
             STYRINGS_OMR.STYREKODE      =    'AT'   )         THEN             
                                                                                
             CALL P020_LES_NESTE_TRANHIST;                                      
          ELSE                                                                  
             CALL P030_LES_TRANHIST_GN_QUAL;                                    
       END;                                                                     
                                                                                
        TO_GRUNNBYP_LEST   = '0'B;                                              
        GRUNNBYP_IKKE_LEST = '1'B;                                              
                                                                                
 /*EE*/ TO_GRUNNEES_LEST   = '0'B;                                              
 /*EE*/ GRUNNEES_IKKE_LEST = '1'B;                                              
                                                                                
        DO   WHILE  (W01.MER_DATA  &  W01.BARN_IKKE_LEST &                      
                     ^TO_GRUNNBYP_LEST & ^TO_GRUNNEES_LEST );                   
                                                                                
      IF FEIL_MELD_NR                    >    0                THEN             
         DO;                                                                    
           /* ********************************************** */                 
           /*  FEIL VED LES                                  */                 
           /* ********************************************** */                 
                                                                                
           GO TO L999;                                                          
         END;                                                                   
                                                                                
                                                                                
      SELECT ( RF1.SEGM_NAVN );                                                 
                                                                                
        WHEN ( 'RF0PERSN' )                                                     
          /* **************************************************** */            
          /*  FÅTT INN :    R F 0 P E R S N                       */            
          /* **************************************************** */            
          DO;                                                                   
                                                                                
             IF W01.PERSON_IKKE_FUNNET                            THEN          
                DO;                                                             
                  SUBSTR (ADDR            (B01.RF0PERSN (14))                   
                  ->  W01_HJELPE_IO,1,STG (B01.RF0PERSN (14))) = W01_IO;        
                                                                                
                  W01.NAVN           =     B01.RF0PERSN (14).NAVN      ;        
                END;                                                            
                                                                                
            TRANS_OPPL_OMR.FØDSNUMMER      =   SEARCH_FNR;                      
                                                                                
            IF STYRINGS_OMR.FUNKSJONSKODE  =   'F'       !                      
              (STYRINGS_OMR.FUNKSJONSKODE  =   'E'       &                      
               STYRINGS_OMR.STYREKODE      =   'AT'      )         THEN         
                                                                                
               CALL P020_LES_NESTE_TRANHIST;                                    
            ELSE                                                                
               CALL P030_LES_TRANHIST_GN_QUAL;                                  
          END;                                                                  
                                                                                
        WHEN ( 'TRANHIST' )                                                     
          /* **************************************************** */            
          /*  FÅTT INN :    T R A N H I S T                       */            
          /* **************************************************** */            
          DO;                                                                   
            SUBSTR (ADDR                (TRANHIST)                              
                ->  W01_HJELPE_IO,1,STG (TRANHIST)) =  W01_IO;                  
            CALL    P010_LES_SEGM;                                              
          END;                                                                  
                                                                                
        WHEN ( 'GRUNNBAP' )                                                     
          /* **************************************************** */            
          /*  FÅTT INN :    G R U N N B A P                       */            
