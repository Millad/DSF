                                                                        00001120
 /* ***************************************************************** */00001130
 /* KALL SOM BEREGNER GRUNNPENSJON FOR BARN1.                         */00001140
 /* GP ER ÅRSBELØP OG IKKE DELT PÅ ANTALL BARN, IKKE FORHØYET.        */00001150
 /* ***************************************************************** */00001160
                                                                        00001170
    CALL BEREGN_GP_BARN1;                                               00001180
                                                                        00001190
