                                                                        00001020
    MOR_IND  = 1;                                                       00001030
    FAR_IND  = 2;                                                       00001040
    VIRK_ÅM  = B02.VIRK_DATO_ÅM(BER_SØ_IND);                            00001050
                                                                        00001060
    W_GP            = 0;                                                00001070
