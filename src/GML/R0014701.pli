 /* KALL SOM BEREGNER TILLEGGSPENSJON FOR BARN.                       */00001210
 /* ***************************************************************** */00001220
 /* W_TP ER MÅNEDSBELØP DELT PÅ ANTALL BARN, MEN IKKE FORHØYET.       */00001230
 /* ***************************************************************** */00001240
                                                                        00001250
    FNR             = B02.FNR(MOR_IND);                                 00001260
    POTALL_OPPL.IND = MOR_IND;                                          00001270
                                                                        00001280
    IF FNR > 0 THEN                                                     00001290
       DO;                                                              00001300
          DØDSÅR    = B02.DØDSDATO_ÅMD(MOR_IND) / 10000;                00001310
          CALL BEREGN_TP_BARN(W_TP_MOR,FNR_R.ÅR,POREKKE(MOR_IND));      00001320
                                                                        00001330
          IF FEIL_MELD_NR > 0  THEN                                     00001340
             GO TO L999;                                                00001350
          ELSE                                                          00001360
             PROGRAM_ID = 'R0014701';                                   00001370
       END;                                                             00001380
                                                                        00001390
    FNR             = B02.FNR(FAR_IND);                                 00001400
    POTALL_OPPL.IND = FAR_IND;                                          00001410
                                                                        00001420
    IF FNR > 0 THEN                                                     00001430
       DO;                                                              00001440
          DØDSÅR    = B02.DØDSDATO_ÅMD(FAR_IND) / 10000;                00001450
          CALL BEREGN_TP_BARN(W_TP_FAR,FNR_R.ÅR,POREKKE(FAR_IND));      00001460
                                                                        00001470
          IF FEIL_MELD_NR > 0  THEN                                     00001480
             GO TO L999;                                                00001490
          ELSE                                                          00001500
             PROGRAM_ID = 'R0014701';                                   00001510
       END;                                                             00001520
                                                                        00001530
    W_ANTALL_BARN   = B02.ANTALL_BARN(BER_SØ_IND);                      00001540
                                                                        00001550
