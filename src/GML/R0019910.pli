 F_NUMERISK:                                                            R0000150
   PROC(NUMFELT) RETURNS (BIT(1));                                      R0000160
       DCL  NUMFELT CHAR(*),                                            R0000170
            RETUR BIT(1) INIT('1'B);                                    R0000180
            IF VERIFY(NUMFELT,'0123456789') > 0 THEN                    R0000190
            RETUR = '0'B;                                               R0000200
            RETURN(RETUR);                                              R0000210
 END F_NUMERISK;                                                        R0000220
