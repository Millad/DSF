 /* ******************************************************** */         00008050
                                                                        00008060
 IF RETT_TIL_ÅFT(IND) = 'J'   THEN                                      00008070
    DO;                                                                 00008080
       GRAD = 100;                                                      00008090
       CALL BT_ET;                                                      00008100
       ÅFT_BT = W_BT / 12 + 0.5;                                        00008110
