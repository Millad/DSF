 P2_TT_BARN : PROC;                                                     00005830
                                                                        00005840
       DO K = 3 TO 14 UNTIL (B02.FNR(K) = B02.FNR_TILKN(IND,I));        00005850
       END;                                                             00005860
       IF B02.PENSJONSRETT_FØR_91(K) = 'J'      THEN                    00005870
          TT_BARN             = 40;                                     00005880
