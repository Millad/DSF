 BT_ET : PROC;                                                          00008400
                                                                        00008410
    IF ANT_TT_RED  > 0               THEN                               00008420
       IF ANT_REDU > 0               THEN                               00008430
          IF ANT_TT_RED  > ANT_REDU  THEN                               00008440
                                                                        00008450
             W_BT = ((ANT_REDU * 34100 *                                00008460
                              BT_PROSENT_RED * TT_FAKTOR/40) +          00008470
                    ((ANT_TT_RED - ANT_REDU) * 34100 * BT_PROSENT *     00008480
                       TT_FAKTOR/40)                              +     00008490
                    ((GARANTI_BARN - ANT_TT_RED)                        00008500
                     * 34100 * BT_PROSENT)) * GRAD / 100;               00008510
          ELSE                                                          00008520
                                                                        00008530
             W_BT = ((ANT_TT_RED * 34100 *                              00008540
                               BT_PROSENT_RED * TT_FAKTOR/40)  +        00008550
                    (ANT_REDU - ANT_TT_RED) * 34100 * BT_PROSENT_RED +  00008560
                    ((GARANTI_BARN - ANT_REDU)                          00008570
                     * 34100 * BT_PROSENT)) * GRAD / 100;               00008580
                                                                        00008590
       ELSE                                                             00008600
                                                                        00008610
          W_BT = ((ANT_TT_RED * 34100 * BT_PROSENT * TT_FAKTOR/40)  +   00008620
                 ((GARANTI_BARN - ANT_TT_RED)                           00008630
                  * 34100 * BT_PROSENT)) * GRAD / 100;                  00008640
                                                                        00008650
    ELSE                                                                00008660
                                                                        00008670
       W_BT = ((ANT_REDU * 34100 * BT_PROSENT_RED )    +                00008680
                 ((GARANTI_BARN - ANT_REDU)                             00008690
                  * 34100 * BT_PROSENT)) * GRAD / 100;                  00008700
                                                                        00008710
    IF EK_0591 = 'J'                          THEN                      00008720
       HJELPEFELT = 34100 * B02.TT_ANV(IND) * GRAD * 0.5;               00008730
                                                                        00008740
 END BT_ET;                                                             00008750
                                                                        00008760
   /*GARANTI_SUM ER MÅNEDSBELØP*/                                       00008770
                                                                        00008780
 GARANTI_SUM = GARANTI_PENSJON + GARANTI_EK + GARANTI_BT                00008790
               + ÅFT_TILLEGG;                                           00008800
