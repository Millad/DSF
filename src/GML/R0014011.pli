 /*PROSEDYRE FOR Å BEREGNE INNTEKT OG FRIBELØP               */         00005980
 /*                                                          */         00005990
 /* REGNER : INNTEKT, INNTEKT_EK, INNTEKT_V, INNTEKT_W,      */         00006000
 /*          FRIBELØP_V, FRIBELØP_W                          */         00006010
 /* ******************************************************** */         00006020
                                                                        00006030
 P3_REGN_INNT_FRIBELØP : PROC;                                          00006040
                                                                        00006050
 INNTEKT       = B02.ARBEIDSINNTEKT(IND) * 100 +                        00006060
                 B02.PENSJONSINNTEKT(IND) * 100 + W_FOLKTRYGD * 12;     00006070
 INNTEKT_EK    = B02.ARBEIDSINNTEKT_EK(IND) * 100 +                     00006080
                 B02.PENSJONSINNTEKT_EK(IND) * 100;                     00006090
 IF INNTEKT_EK > G   THEN                                               00006100
    INNTEKT_EK = INNTEKT_EK + FOLKETRYGD_EK_ÅR - G;                     00006110
 ELSE                                                                   00006120
    INNTEKT_EK = FOLKETRYGD_EK_ÅR;                                      00006130
                                                                        00006140
 IF TOTALT_V > 0            THEN                                        00006150
    DO;                                                                 00006160
       IF B02.PENSJONSTYPE2(IND) = 'F'             &                    00006170
          B02.SIVILSTAND   (IND) = 'A'             THEN                 00006180
          DO;                                                           00006190
 /*          HJELPEFELT = G * (1.75 + (PROSENT_ST * 2 /100)             00006200
                        + 0.25 * TOTALT_V)                              00006210
                       * TT_FAKTOR / 40 + 0.5;                */        00006220
             HJELP2     = PROSENT_ST * 2 /100;                          00006230
             IF TOTALT_V > 1            THEN       /*9805*/             00006240
                HJELP3     = BT_PROSENT * (TOTALT_V - 1);               00006250
             HJELP4     = 2 + HJELP2 + HJELP3;                          00006260
             HJELP5     = G * HJELP4;                                   00006270
             HJELP6     = HJELP5 * TT_FAKTOR/ 40 + 0.5;                 00006280
                                                                        00006290
             FULL_V = FULL_ET + BT_V;                                   00006300
             FULL_W = BT_W;                                             00006310
          END;                                                          00006320
       ELSE                                                             00006330
          DO;                                                           00006340
 /*          HJELPEFELT = G * (1 + (ST_PROSENT_ENSLIG / 100)            00006350
                        + 0.25 * TOTALT_V)                              00006360
                        * TT_FAKTOR / 40 + 0.5;        */               00006370
             HJELP2     = ST_PROSENT_ENSLIG /100;                       00006380
             IF TOTALT_V > 1            THEN       /*9805*/             00006390
                HJELP3     = BT_PROSENT * (TOTALT_V - 1);               00006400
             HJELP4     = 1.50 + HJELP2 + HJELP3;                       00006410
                                                                        00006420
   /* OBS OBS  LEGGES INN ETTER OMREGNINGEN  :                 */       00006430
   /*                                                          */       00006440
             IF G > 35500 THEN            /*  LOVENDRING 1.5.92*/       00006450
                HJELP4 = HJELP4 + 0.25;                                 00006460
             HJELP5     = G * HJELP4;                                   00006470
             HJELP6     = HJELP5 * TT_FAKTOR/ 40 + 0.5;                 00006480
             FULL_V = BT_V;                                             00006490
             FULL_W = BT_W + FULL_ET;                                   00006500
          END;                                                          00006510
       FRIBELØP_V = HJELP6;                                             00006520
       DIV_PARAM_OMR.FRIBELØP_SÆRBARN(IND) = FRIBELØP_V;                00006530
    END;                                                                00006540
 ELSE                                                                   00006550
    FULL_W = BT_W + FULL_ET;                                            00006560
                                                                        00006570
 IF TOTALT_W > 0 !                                                      00006580
    FULL_ET  > 0             THEN                                       00006590
    DO;                                                                 00006600
 /*    HJELPEFELT = G * (1.75 + (ST_PROSENT_SAMST * 2 /100)             00006610
                     + 0.25 * TOTALT_W)                                 00006620
                    * TT_FAKTOR / 40 + 0.5;                   */        00006630
             HJELP2     = PROSENT_ST * 2 /100;                          00006640
             HJELP3     = BT_PROSENT * TOTALT_W;                        00006650
             HJELP4     = 1.75 + HJELP2 + HJELP3;                       00006660
             HJELP5     = G * HJELP4;                                   00006670
             HJELP6     = HJELP5 * TT_FAKTOR/ 40 + 0.5;                 00006680
       FRIBELØP_W = HJELP6;                                             00006690
       DIV_PARAM_OMR.FRIBELØP_FELLESBARN(IND) = FRIBELØP_W;             00006700
    END;                                                                00006710
                                                                        00006720
 IF FRIBELØP_V > 0             &                                        00006730
    FRIBELØP_W > 0             &                                        00006740
    INNTEKT > FRIBELØP_V       &                                        00006750
    INNTEKT > FRIBELØP_W             THEN                               00006760
    DO;                                                                 00006770
       IF FRIBELØP_W > FRIBELØP_V                 THEN                  00006780
             FORDELINGS_INNT = INNTEKT - FRIBELØP_W;                    00006790
       ELSE                                                             00006800
             FORDELINGS_INNT = INNTEKT - FRIBELØP_V;                    00006810
                                                                        00006820
       HJELPEFELT      = FORDELINGS_INNT * FULL_V  /                    00006830
                         (FULL_V + FULL_W) + 0.5;                       00006840
       TILLEGGSINNT_V  = HJELPEFELT;                                    00006850
       TILLEGGSINNT_W  = FORDELINGS_INNT - TILLEGGSINNT_V;              00006860
       INNTEKT_V = INNTEKT + TILLEGGSINNT_V - FORDELINGS_INNT;          00006870
       INNTEKT_W = INNTEKT + TILLEGGSINNT_W - FORDELINGS_INNT;          00006880
    END;                                                                00006890
                                                                        00006900
 ELSE                                                                   00006910
    DO;                                                                 00006920
       INNTEKT_V = INNTEKT;                                             00006930
       INNTEKT_W = INNTEKT;                                             00006940
    END;                                                                00006950
  /* **********                                                         00006960
 IF FRIBELØP_V > FRIBELØP_W THEN                                        00006970
    IF FRIBELØP_W > 0          THEN                                     00006980
       DIV_PARAM_OMR.FRIBELØP_MINSTE = FRIBELØP_W;                      00006990
    ELSE                                                                00007000
       DIV_PARAM_OMR.FRIBELØP_MINSTE = FRIBELØP_V;                      00007010
 ELSE                                                                   00007020
    IF FRIBELØP_V > 0          THEN                                     00007030
       DIV_PARAM_OMR.FRIBELØP_MINSTE = FRIBELØP_V;                      00007040
    ELSE                                                                00007050
       DIV_PARAM_OMR.FRIBELØP_MINSTE = FRIBELØP_W;                      00007060
               ****** SLIK VAR DET . TRUDE 031091   *********   */      00007070
  /* ********** LAGET INDEX PÅ FRIBELØPENE :   TRUDE,031091  */         00007080
                                                                        00007090
 IF FRIBELØP_V > FRIBELØP_W THEN                                        00007100
    IF FRIBELØP_W > 0          THEN                                     00007110
       DIV_PARAM_OMR.FRIBELØP_MINSTE(IND) = FRIBELØP_W;                 00007120
    ELSE                                                                00007130
       DIV_PARAM_OMR.FRIBELØP_MINSTE(IND) = FRIBELØP_V;                 00007140
 ELSE                                                                   00007150
    IF FRIBELØP_V > 0          THEN                                     00007160
       DIV_PARAM_OMR.FRIBELØP_MINSTE(IND) = FRIBELØP_V;                 00007170
    ELSE                                                                00007180
       DIV_PARAM_OMR.FRIBELØP_MINSTE(IND) = FRIBELØP_W;                 00007190
                 /* TIL HIT 3.10.91  TRUDE */                           00007200
 END P3_REGN_INNT_FRIBELØP;                                             00007210
                                                                        00007220
 /* ******************************************************** */         00007230
 /*PROSEDYRE FOR Å BEREGNE GARANTERT      FORSØRGINGSTILLEGG */         00007240
 /* ******************************************************** */         00007250
                                                                        00007260
 P4_REGN_GARANTISUM : PROC;                                             00007270
                                                                        00007280
 HJELPEFELT  = 0;                                                       00007290
 IF W_TP_GARANTI > 0           THEN                                     00007300
    GARANTI_PENSJON = (W_FOLKTRYGD - W_TP_GARANTI) * 34100/G + 1        00007310
                       + W_TP_GARANTI;                                  00007320
 ELSE                                                                   00007330
 /* IF B02.GT_LOV92(IND) > TP_ETTER92   THEN   */                       00007340
 IF B02.GT_LOV92(IND) > 0             THEN                              00007350
    DO;                                                                 00007360
       TP1 = B02.GT_LOV92(IND) * 341/355 + 0.5;                         00007370
       TP2 = TP_ETTER92 * 34100/G + 0.5;                                00007380
       IF B02.TP_NETTO(IND) < B02.TP_BRUTTO(IND) THEN                   00007390
          TP1 = TP1 - (B02.TP_BRUTTO(IND) - B02.TP_NETTO(IND))          00007400
                                  * 34100/G;                            00007410
       ELSE                                                             00007420
                                                                        00007430
 /*TILLEGG 18.9.92 HL */                                                00007440
                                                                        00007450
       IF B02.YRKEPENS.TP(IND) > 0             THEN                     00007460
          TP1 = (B02.GT_LOV92(IND) - B02.YRKEPENS.TP(IND)) * 341/355    00007470
                + B02.YRKEPENS.TP(IND) * 34100/G  + 0.5;                00007480
 /*HIT HL */                                                            00007490
       ELSE /*HL 6.9.93*/                                               00007500
       IF B02.AP_TP_NETTO(IND) < B02.ALDERSP.TP(IND)   THEN             00007510
          TP1 = B02.GT_LOV92(IND) * 341/355 * GRAD/100 + 0.5;           00007520
 /*       TP1 = TP1 - (B02.ALDERSP.TP(IND) - B02.AP_TP_NETTO(IND))      00007530
                                  * 34100/G;                  */        00007540
 /*GARANTIEN PÅ TP SKAL VÆRE DIFFERANSEN MELLOM GT_LOV92 OG   */        00007550
 /*BRUTTO TP (BRUTTO_TP OMREGNET ETTER G=34100), MED FRADRAG  */        00007560
 /*AV DET BELØP SOM FAKTISK ER TRUKKET I TILLEGGSPENSJONEN    */        00007570
 /*       TP_TREKK = B02.ALDERSP.TP(IND) - B02.AP_TP_NETTO(IND);*/      00007580
 /*       TP_DIFF  = TP1 - (TP_ETTER92 * 34100/G) - TP_TREKK;   */      00007590
                                                                        00007600
     /*HIT HL */                                                        00007610
                                                                        00007620
       IF TP1 > TP2 THEN                                                00007630
          GARANTI_PENSJON = (W_FOLKTRYGD - TP_ETTER92 -                 00007640
                B02.GT_TILLEGG_LOV92(IND)) * 34100/G + TP1 + 1;         00007650
       ELSE                                                             00007660
          GARANTI_PENSJON = W_FOLKTRYGD * 34100/G + 1;                  00007670
    END;                                                                00007680
 ELSE                                                                   00007690
    DO;                                                                 00007700
       GARANTI_PENSJON = W_FOLKTRYGD * 34100 / G + 1;                   00007710
       B02.GT_TILLEGG_LOV92(IND) = 0;                                   00007720
