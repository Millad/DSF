 /* REGNER: B02.GARANTITILLEGG_EK, B02.GARANTITILLEGG_BARN OG*/         00011130
 /*         B02.GARANTITILLEGG_SÆRBARN                      */          00011140
 /* ******************************************************** */         00011150
                                                                        00011160
 P6_GARANTITILLEGG : PROC;                                              00011170
                                                                        00011180
  GARANTITILLEGG       = GARANTI_SUM - REDUKSJONS_BELØP;                00011190
  B02.SUM_YTELSE (IND) =  B02.SUM_YTELSE(IND) + GARANTITILLEGG;         00011200
  IF ÅFT_TILLEGG > 0        THEN                                        00011210
     DO;                                                                00011220
        IF ÅFT_TILLEGG > GARANTITILLEGG    THEN                         00011230
           DO;                                                          00011240
              B02.ÅFT(IND)   = B02.ÅFT(IND) + GARANTITILLEGG;           00011250
              GARANTITILLEGG = 0;                                       00011260
           END;                                                         00011270
        ELSE                                                            00011280
           DO;                                                          00011290
              B02.ÅFT(IND)   = B02.ÅFT(IND) + ÅFT_TILLEGG;              00011300
              GARANTITILLEGG = GARANTITILLEGG - ÅFT_TILLEGG;            00011310
           END;                                                         00011320
     END;                                                               00011330
  REDUSERT_EK = GARANTI_EK - REDUKSJON_ET;                              00011340
                                                                        00011350
 /*NY BEREGNING AV FORDELING AV GARANTI - HL 25.9.92 : */               00011360
                                                                        00011370
 /* ********************************                                    00011380
  IF BARN_0591 > 0 THEN                                                 00011390
     HJELPEFELT  = GARANTI_BT * ANT_W/BARN_0591 + 0.5                   00011400
                   - REDUKSJON_FB                                       00011410
  ELSE                                                                  00011420
     HJELPEFELT = 0                                                     00011430
  REDUSERT_FB = HJELPEFELT                                              00011440
  REDUSERT_SB = GARANTI_BT - REDUKSJON_SB - REDUSERT_FB - REDUKSJON_FB  00011450
                                                                        00011460
    ************************************************* */                00011470
                                                                        00011480
  REDUSERT_FB = GARANTI_BT_W - REDUKSJON_FB;                            00011490
  REDUSERT_SB = GARANTI_BT_V - REDUKSJON_SB;                            00011500
                                                                        00011510
 /* HIT HL */                                                           00011520
                                                                        00011530
  REDUSERT_TOT = REDUSERT_EK + REDUSERT_FB + REDUSERT_SB;               00011540
  IF REDUSERT_EK > 0                  THEN                              00011550
     DO;                                                                00011560
        B02.GARANTITILLEGG_EK(IND) =                                    00011570
            GARANTITILLEGG * REDUSERT_EK/REDUSERT_TOT + 0.5;            00011580
        IF REDUSERT_FB > 0               THEN                           00011590
           IF REDUSERT_SB > 0            THEN                           00011600
              DO;                                                       00011610
                 B02.GARANTITILLEGG_BARN(IND)= GARANTITILLEGG           00011620
                  * REDUSERT_FB/REDUSERT_TOT + 0.5;                     00011630
                 B02.GARANTITILLEGG_SÆRBARN(IND) = GARANTITILLEGG       00011640
                           - B02.GARANTITILLEGG_EK(IND)                 00011650
                           - B02.GARANTITILLEGG_BARN(IND);              00011660
              END;                                                      00011670
           ELSE                                                         00011680
              B02.GARANTITILLEGG_BARN(IND) = GARANTITILLEGG             00011690
                           - B02.GARANTITILLEGG_EK(IND);                00011700
        ELSE                                                            00011710
        IF REDUSERT_SB > 0 THEN                                         00011720
           B02.GARANTITILLEGG_SÆRBARN(IND) = GARANTITILLEGG             00011730
                           - B02.GARANTITILLEGG_EK(IND);                00011740
     END;                                                               00011750
  ELSE                                                                  00011760
     IF REDUSERT_FB > 0               THEN                              00011770
        IF REDUSERT_SB > 0            THEN                              00011780
           DO;                                                          00011790
              B02.GARANTITILLEGG_BARN(IND)= GARANTITILLEGG              00011800
                  * REDUSERT_FB/REDUSERT_TOT + 0.5;                     00011810
              B02.GARANTITILLEGG_SÆRBARN(IND) = GARANTITILLEGG          00011820
                           - B02.GARANTITILLEGG_BARN(IND);              00011830
           END;                                                         00011840
        ELSE                                                            00011850
           B02.GARANTITILLEGG_BARN(IND) = GARANTITILLEGG;               00011860
     ELSE                                                               00011870
        B02.GARANTITILLEGG_SÆRBARN(IND) = GARANTITILLEGG;               00011880
                                                                        00011890
 END P6_GARANTITILLEGG;                                                 00011900
