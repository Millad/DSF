       ÅFT_FORS_TILLEGG = ÅFT_ET + ÅFT_BT - GARANTI_BT - GARANTI_EK;    00008130
       IF B02.PENSJONSTYPE2(IND) = 'S' THEN                             00008140
          DO;                                                           00008150
             MINSTE_ST = 34100 * PROSENT_ST * TT_FAKTOR                 00008160
                          / (100 * 40 * 12) + 0.5;                      00008170
             MINSTE_GP = 34100 * 0.75 / 12 + 0.5;                       00008180
          END;                                                          00008190
       ELSE                                                             00008200
          DO;                                                           00008210
             MINSTE_ST = 34100 * ST_PROSENT_ENSLIG * TT_FAKTOR          00008220
                          / (100 * 40 * 12) + 0.5;                      00008230
             MINSTE_GP = 34100 / 12 + 0.5;                              00008240
          END;                                                          00008250
       MINSTEPENSJON = MINSTE_GP + MINSTE_ST;                           00008260
       IF GARANTI_PENSJON > MINSTEPENSJON        THEN                   00008270
          ÅFT_FRADRAG = GARANTI_PENSJON - MINSTEPENSJON;                00008280
       ELSE                                                             00008290
          ÅFT_FRADRAG = 0;                                              00008300
       ÅFT_TILLEGG = ÅFT_FORS_TILLEGG - ÅFT_FRADRAG;                    00008310
