 P2_TELL_FORSØRGEDE : PROC;                                             00004610
                                                                        00004620
  /*REGNER OGSÅ GARANTI BARNETILLEGG, FORDELT PÅ V/W      */            00004630
  /*OPPDATERE KODER, NÅR DETTE IKKE KAN VÆRE GJORT FRA FØR*/            00004640
  /*FT_FØR_91 FOR FORSØRGET EKTEFELLE (F) OG BARN (R,L)   */            00004650
  /*TILKNYTNINGSKODE FOR BARN (W)                         */            00004660
  /*PENSJONSTYPE2    FOR BARN (W)                         */            00004670
                                                                        00004680
 IF B02.FT_SKAL_BEREGNES(IND) = 'N'         THEN                        00004690
 /* B01.FT_SKAL_BEREGNES(IND) = ' '         THEN       */               00004700
    DO;                                                                 00004710
       IF B02.PENSJONSTYPE1(IND_EK) = 'F'    THEN                       00004720
          DO J = 1 TO 13 WHILE(B02.FNR_TILKN(IND,J) > 0);               00004730
             IF B02.FNR(IND_EK)= B02.FNR_TILKN(IND,J) THEN              00004740
                DO;                                                     00004750
                   B02.FT_FØR_91(IND,J)   = 'F';                        00004760
                   J=13;                                                00004770
                END;                                                    00004780
          END;                                                          00004790
       DO I = 3 TO 14 WHILE(B02.FNR(I) > 0);                            00004800
          DO J = 1 TO 13 WHILE(B02.FNR_TILKN(IND,J) > 0);               00004810
             IF B02.FNR(I) = B02.FNR_TILKN(IND,J)   THEN                00004820
               DO;                                                      00004830
                 IF B02.FT_FØR_91(IND,J) = ' '     THEN                 00004840
                   DO;                                                  00004850
                      IF B02.PENSJONSTYPE2(I)         = 'R'   &         00004860
                         B02.PENSJONSTYPE2(IND_EK)    = 'M'   THEN      00004870
                         DO;                                            00004880
                            B02.FT_FØR_91(IND,J)      = 'R';            00004890
                            B02.TILKNYTNINGSKODE(IND,J)   = 'W';        00004900
                            B02.PENSJONSTYPE2(I)   = 'W';               00004910
                         END;                                           00004920
                      ELSE                                              00004930
                      IF B02.PENSJONSTYPE1(I)         = 'L'   THEN      00004940
                         DO;                                            00004950
                            B02.FT_FØR_91(IND,J)   = 'L';               00004960
                            B02.TILKNYTNINGSKODE(IND,J)   = 'W';        00004970
                            B02.PENSJONSTYPE2(I)   = 'W';               00004980
                         END;                                           00004990
                   END;                                                 00005000
                 J=13;                                                  00005010
               END;                                                     00005020
          END;                                                          00005030
       END;                                                             00005040
    END;                                                                00005050
                                                                        00005060
 /*OPPTELLING AV FORSØRGEDE FØR OG ETTER 0591*/                         00005070
                                                                        00005080
                                                                        00005090
 DO I = 1 TO 13 WHILE (B02.FNR_TILKN(IND,I) > 0);                       00005100
                                                                        00005110
    IF B02.FT_FØR_91(IND,I) = 'F'               THEN                    00005120
       DO;                                                              00005130
          REGN_GARANTI = 'J';                                           00005140
          EK_0591      = 'J';                                           00005150
       END;                                                             00005160
    ELSE                                                                00005170
    IF B02.FT_FØR_91(IND,I) = 'L'               THEN                    00005180
       DO;                                                              00005190
          REGN_GARANTI = 'J';                                           00005200
          BARN_FULL = BARN_FULL + 1;                                    00005210
          BARN_0591 = BARN_0591 + 1;                                    00005220
          IF  B02.ANTALL_BARN(IND) > FULL_TT     THEN                   00005230
             CALL P2_TT_BARN;                                           00005240
          ELSE                                                          00005250
             TT_BARN = 40;                                              00005260
          IF B02.TILKNYTNINGSKODE(IND,I) = 'W'  THEN                    00005270
             DO;                                                        00005280
                ANT_W = ANT_W + 1;                                      00005290
                HJELP_BT_W = HJELP_BT_W +                               00005300
                   34100 * TT_BARN/40 * BT_PROSENT * GRAD/100;          00005310
             END;                                                       00005320
          ELSE                                                          00005330
             DO;                                                        00005340
                ANT_V = ANT_V + 1;                                      00005350
                HJELP_BT_V = HJELP_BT_V +                               00005360
                   34100 * TT_BARN/40 * BT_PROSENT * GRAD/100;          00005370
             END;                                                       00005380
       END;                                                             00005390
    ELSE                                                                00005400
    IF B02.FT_FØR_91(IND,I) = 'R'               THEN                    00005410
       DO;                                                              00005420
          REGN_GARANTI = 'J';                                           00005430
          ANT_REDU  = ANT_REDU  + 1;                                    00005440
          BARN_0591 = BARN_0591 + 1;                                    00005450
          IF  B02.ANTALL_BARN(IND) > FULL_TT     THEN                   00005460
             CALL P2_TT_BARN;                                           00005470
          ELSE                                                          00005480
             TT_BARN = 40;                                              00005490
          IF B02.TILKNYTNINGSKODE(IND,I) = 'W'  THEN                    00005500
             DO;                                                        00005510
                ANT_W = ANT_W + 1;                                      00005520
                HJELP_BT_W = HJELP_BT_W +                               00005530
                  34100 * TT_BARN/40 * BT_PROSENT_RED * GRAD/100;       00005540
             END;                                                       00005550
          ELSE                                                          00005560
             DO;                                                        00005570
                ANT_V = ANT_V + 1;                                      00005580
                HJELP_BT_V = HJELP_BT_V +                               00005590
                  34100 * TT_BARN/40 * BT_PROSENT_RED * GRAD/100;       00005600
             END;                                                       00005610
       END;                                                             00005620
    ELSE                                                                00005630
    IF B02.TILKNYTNINGSKODE(IND,I) = 'W'  THEN                          00005640
       TILLEGG_W = TILLEGG_W + 1;                                       00005650
    ELSE                                                                00005660
    IF B02.TILKNYTNINGSKODE(IND,I) = 'V'  THEN                          00005670
       TILLEGG_V = TILLEGG_V + 1;                                       00005680
                                                                        00005690
 END;                                                                   00005700
                                                                        00005710
 GARANTI_BT_V = HJELP_BT_V / 12 + 0.5;                                  00005720
 GARANTI_BT_W = HJELP_BT_W / 12 + 0.5;                                  00005730
 TOTALT_W = ANT_W + TILLEGG_W;                                          00005740
 TOTALT_V = ANT_V + TILLEGG_V;                                          00005750
                                                                        00005760
 IF (TOTALT_W + TOTALT_V) > 0      THEN                                 00005770
    DO;                                                                 00005780
       BT_W = FULL_BT * TOTALT_W / ( TOTALT_W + TOTALT_V) + 0.5;        00005790
       BT_V = FULL_BT - BT_W;                                           00005800
    END;                                                                00005810
