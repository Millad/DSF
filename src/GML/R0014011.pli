 /* ******************************************************** */         00008840
 /*      PROSEDYRE FOR Å BEREGNE REDUKSJONSBELØPET           */         00008850
 /*                                                          */         00008860
 /* REGNER     : REDUKSJON_ET, REDUKSJON_FB, REDUKSJON_SB    */         00008870
 /* REGNER     : RETUR_ET, RETUR_FB OG RETUR_SB              */         00008880
 /* OPPDATERER : B02.ET, B02.BT, B02.SB OG B02.SUM_YTELSE    */         00008890
 /*              MED RETUR-BELØPENE                          */         00008900
 /* ******************************************************** */         00008910
                                                                        00008920
 P5_REGN_REDUKSJON : PROC;                                              00008930
                                                                        00008940
                                                                        00008950
 IF B02.ANTALL_BARN(IND) = 0        THEN /*BARE RETUR_ET*/              00008960
    CALL P51_EK;                                                        00008970
 ELSE                                                                   00008980
    DO;                                                                 00008990
       IF B02.PENSJONSTYPE2(IND) = 'F'           THEN                   00009000
          IF TOTALT_V > 0                           THEN                00009010
             IF B02.SIVILSTAND(IND) = 'A'              THEN             00009020
                CALL P52_EK_V;                                          00009030
             ELSE                                                       00009040
                DO;                                                     00009050
                   CALL P55_V;                                          00009060
                   IF TOTALT_W > 0                           THEN       00009070
                      CALL P53_EK_W;                                    00009080
                   ELSE                                                 00009090
                      CALL P51_EK;                                      00009100
                END;                                                    00009110
          ELSE                                                          00009120
          IF TOTALT_W > 0                           THEN                00009130
             CALL P53_EK_W;                                             00009140
          ELSE                                                          00009150
             CALL P51_EK;                                               00009160
       ELSE                                                             00009170
       IF TOTALT_W > 0                           THEN                   00009180
          DO;                                                           00009190
             CALL P54_W;                                                00009200
             IF TOTALT_V > 0                        THEN                00009210
                CALL P55_V;                                             00009220
          END;                                                          00009230
       ELSE                                                             00009240
       IF TOTALT_V > 0                           THEN                   00009250
          CALL P55_V;                                                   00009260
                                                                        00009270
    END;                                                                00009280
                                                                        00009290
    /* IKKE RETT TIL ÅFT_TILLEGG NÅR BT OSV. = 0     */                 00009300
    /* PENSJONSKTR 23.09.96 I TELF. TRUDE            */                 00009310
                                                                        00009320
    IF (RETUR_SB + RETUR_FB + RETUR_ET )  = 0 THEN                      00009330
      DO;                                                               00009340
        GARANTI_SUM  = GARANTI_SUM - ÅFT_TILLEGG;                       00009350
        ÅFT_TILLEGG  = 0;                                               00009360
      END;                                                              00009370
    /* TRUDE 24.09.96 */                                                00009380
                                                                        00009390
    REDUKSJONS_BELØP = W_FOLKTRYGD + REDUKSJON_ET + REDUKSJON_FB        00009400
                       + REDUKSJON_SB;                                  00009410
                                                                        00009420
   IF (RETUR_SB = BT_V)   &                                             00009430
      (BT_V > 0 )    THEN DIV_PARAM_OMR.FULLT_BT_V  = JA;               00009440
   IF (RETUR_FB = BT_W)   &                                             00009450
      (BT_W > 0 )    THEN DIV_PARAM_OMR.FULLT_BT_W  = JA;               00009460
   IF (RETUR_ET = FULL_ET)   &                                          00009470
      (FULL_ET > 0 )    THEN DIV_PARAM_OMR.FULLT_ET = JA;               00009480
                                                                        00009490
   B02.BT_SÆRBARN(IND)  = RETUR_SB;                                     00009500
   B02.SUM_YTELSE (IND) = B02.SUM_YTELSE (IND) + RETUR_SB;              00009510
                                                                        00009520
 SELECT (B02.PENSJONSTYPE1(IND));                                       00009530
                                                                        00009540
   WHEN ('A','K')                                                       00009550
      DO;                                                               00009560
         B02.SUM_YTELSE(IND) = B02.SUM_YTELSE(IND)                      00009570
               - B02.ALDERSP.ET (IND) - B02.ALDERSP.BT(IND)             00009580
               + RETUR_ET             + RETUR_FB;                       00009590
         B02.ALDERSP.ET (IND) = RETUR_ET;                               00009600
         B02.ALDERSP.BT (IND) = RETUR_FB;                               00009610
      END;                                                              00009620
                                                                        00009630
   WHEN ('U','Y')                                                       00009640
      DO;                                                               00009650
         B02.SUM_YTELSE (IND) = B02.SUM_YTELSE (IND)                    00009660
               - B02.UFØRPENS.ET (IND) - B02.UFØRPENS.BT(IND)           00009670
                                       - B02.YRKEPENS.BT(IND)           00009680
               + RETUR_ET             + RETUR_FB;                       00009690
         B02.UFØRPENS.ET(IND) = RETUR_ET;                               00009700
         B02.UFØRPENS.BT(IND) = RETUR_FB;                               00009710
         B02.YRKEPENS.BT(IND) = 0;                                      00009720
      END;                                                              00009730
                                                                        00009740
 END; /*SELECT*/                                                        00009750
