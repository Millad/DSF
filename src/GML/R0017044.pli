                                                                        00034430
        SELECT (B02.GUNSTIGSTE_ALTERNATIV(IND_EK));                     00034440
           WHEN (2)                                                     00034450
              DO;  /* PRO RATA 1 VINNER */                              00034460
                 /* VED FASTSETTELSEN AV DERES GRUNNPENSJON */          00034470
                 TEKSTTYPE_U_KODE(J) = 'U570';                          00034480
