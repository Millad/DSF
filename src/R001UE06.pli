      IF FRA_UTEN_DIALOG THEN       /* BLOKKER ALT SAMMEN   */                  
         DO;                                                                    
           S001UE1O.STATSBORGERA        = DFHBMASK;                             
           S001UE1O.BOSTEDA             = DFHBMASK;                             
           S001UE1O.NORDISK_TPA         = DFHBMASK;                             
           S001UE1O.NORDISK_LANDA       = DFHBMASK;                             
           S001UE1O.EØS_PENSJONA        = DFHBMASK;                             
           S001UE1O.INNT_PRØVET_EØS_PENSA = DFHBMASK;                           
           S001UE1O.TT_EØS_ANT_ÅRA      = DFHBMASK;                             
           S001UE1O.TT_EØS_ANT_MNDA     = DFHBMASK;                             
         END;                                                                   
   END OVERFØR_EN_SPES_MAP;                                                     
                                                                                
