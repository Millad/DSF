      ELSE                                                                      
         DO;                                                                    
            S001UE1O.EK_INNT_OVER_2GA = DFHBMBRY;                               
            S001UE1I.EK_INNT_OVER_2GL = CURSOR_POS;                             
            IF ENS.EK_INNT_OVER_2GNR ^= 999 THEN                                
