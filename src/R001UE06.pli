         S001UE1O.STATSBORGERA = NOR_NUM;                                       
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.STATSBORGERA = BRI_NUM;                                    
            S001UE1I.STATSBORGERL = CURSOR_POS;                                 
            IF EN_SPES.STATSBORGERNR ^= 999 THEN                                
