 OVERFØR_ENKAP10_MAP:                                                           
   PROC;                                                                        
   DCL                                                                          
      NORMAL  CHAR (1) INIT(' '),                                               
      NOR_NUM CHAR (1) INIT('&'),                                               
      BRI_NUM CHAR (1) INIT('Q');                                               
      S001UE1O.FORVENTET_INNTO =                                                
                    F_FELT_PIC_CHAR7(ENK10.FORVENTET_INNT);                     
      S001UE1O.O_TIL_PENSJONO = ENK10.O_TIL_PENSJON;                            
      S001UE1O.FRADRAG_2GO    = ENK10.FRADRAG_2G ;                              
      IF F_FELT_PIC_CHAR8(ENK10.FRAML_DATO_ÅMD) ^= '00000000' THEN              
        S001UE1O.FRAML_DATO_MÅO = KONV_HÅMD_MÅ(ENK10.FRAML_DATO_ÅMD);           
      S001UE1O.OPPHØR_EOO = ENK10.OPPHØR_EO;                                    
 /*  MARKERING AV ATTRIBUTTER   */                                              
      IF ENK10.FORVENTET_INNTNR = 0 THEN                                        
         S001UE1O.FORVENTET_INNTA = NOR_NUM;                                    
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.FORVENTET_INNTA = BRI_NUM;                                 
            S001UE1I.FORVENTET_INNTL = CURSOR_POS;                              
            IF ENK10.FORVENTET_INNTNR ^= 999 THEN                               
               CALL SKRIV_FEIL((ENK10.FORVENTET_INNTNR));                       
         END;                                                                   
      IF ENK10.O_TIL_PENSJONNR = 0 THEN                                         
         S001UE1O.O_TIL_PENSJONA = NORMAL;                                      
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.O_TIL_PENSJONA = DFHBMBRY;                                 
            S001UE1I.O_TIL_PENSJONL = CURSOR_POS;                               
            IF ENK10.O_TIL_PENSJONNR ^= 999 THEN                                
               CALL SKRIV_FEIL((ENK10.O_TIL_PENSJONNR));                        
         END;                                                                   
      IF ENK10.FRADRAG_2GNR = 0 THEN                                            
         S001UE1O.FRADRAG_2GA = NORMAL ;                                        
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.FRADRAG_2GA = DFHBMBRY;                                    
            S001UE1I.FRADRAG_2GL = CURSOR_POS;                                  
            IF ENK10.FRADRAG_2GNR ^= 999 THEN                                   
               CALL SKRIV_FEIL((ENK10.FRADRAG_2GNR));                           
         END;                                                                   
      IF ENK10.FRAML_DATO_ÅMDNR = 0 THEN                                        
         S001UE1O.FRAML_DATO_MÅA = NOR_NUM;                                     
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.FRAML_DATO_MÅA = BRI_NUM;                                  
            S001UE1I.FRAML_DATO_MÅL = CURSOR_POS;                               
            IF ENK10.FRAML_DATO_ÅMDNR ^= 999 THEN                               
               CALL SKRIV_FEIL((ENK10.FRAML_DATO_ÅMDNR));                       
         END;                                                                   
      IF ENK10.OPPHØR_EONR = 0 THEN                                             
         S001UE1O.OPPHØR_EOA = NORMAL;                                          
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.OPPHØR_EOA = DFHBMBRY;                                     
            S001UE1I.OPPHØR_EOL = CURSOR_POS;                                   
            IF ENK10.OPPHØR_EONR ^= 999 THEN                                    
               CALL SKRIV_FEIL((ENK10.OPPHØR_EONR));                            
         END;                                                                   
                                                                                
      IF FRA_UTEN_DIALOG THEN        /* BLOKKER ALT SAMMEN    */                
         DO;                                                                    
           S001UE1O.REGDATOA       = DFHBMASK;                                  
           S001UE1O.TERMIDA        = DFHBMASK;                                  
           S001UE1O.FORVENTET_INNTA = DFHBMASK;                                 
           S001UE1O.O_TIL_PENSJONA = DFHBMASK;                                  
           S001UE1O.FRADRAG_2GA    = DFHBMASK;                                  
           S001UE1O.FRAML_DATO_MÅA = DFHBMASK;                                  
           S001UE1O.OPPHØR_EOA     = DFHBMASK;                                  
         END;                                                                   
                                                                                
   END OVERFØR_ENKAP10_MAP;                                                     
