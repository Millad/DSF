           S001UE1O.OVER4GA        = DFHBMASK;                                  
           MELDING6O = 'TRYKK PF1 FOR OPPBEVAR, PF2 FOR FORKASTE.';             
         END;                                                                   
                                                                                
      IF ^ FRA_CICS THEN                                                        
         DO;                                                                    
