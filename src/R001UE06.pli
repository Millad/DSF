           S001UE1O.NAVN_NYTT_EKA   = DFHBMASK;                                 
           S001UE1O.FNR_EKA         = DFHBMASK;                                 
           S001UE1O.NAVN_EKA        = DFHBMASK;                                 
           S001UE1O.SKILSMISSEA     = DFHBMASK;                                 
           S001UE1O.SIVILSTANDA     = DFHBMASK;                                 
           S001UE1O.FLYTTET_FRAA    = DFHBMASK;                                 
           S001UE1O.FLYTTET_SAMMENA = DFHBMASK;                                 
           S001UE1O.EK_INNT_OVER_2GA = DFHBMASK;                                
