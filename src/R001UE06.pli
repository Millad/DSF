         END;                                                                   
                                                                                
      /* VED GAMLE TRANS SKAL FNR OG VIRKNINGSDATO BLOKKERES   */               
                                                                                
      IF TRANS_OPPL_OMR.NY_GML_TRANS_IND = 'G' & FRA_MED_DIALOG THEN            
         DO;                                                                    
           S001UE1O.FNRA            = DFHBMASK; /* PROT AND ASKIP */            
           S001UE1O.VIRK_DATO_MÅA   = DFHBMASK;                                 
         END;                                                                   
                                                                                
      ELSE IF FRA_UTEN_DIALOG THEN  /* BLOKKER ALT SAMMEN  */                   
                                                                                
         DO;                                                                    
           S001UE1O.REGDATOA        = DFHBMASK;                                 
           S001UE1O.TERMIDA         = DFHBMASK;                                 
           S001UE1O.FNRA            = DFHBMASK;                                 
           S001UE1O.TKNRA           = DFHBMASK;                                 
           S001UE1O.SPRÅKA          = DFHBMASK;                                 
