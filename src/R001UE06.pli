         END;                                                                   
      IF ENS.SKILSMISSENR = 0 THEN                                              
         S001UE1O.SKILSMISSEA = NORMAL;                                         
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.SKILSMISSEA = DFHBMBRY;                                    
            S001UE1I.SKILSMISSEL = CURSOR_POS;                                  
            IF ENS.SKILSMISSENR ^= 999 THEN                                     
               CALL SKRIV_FEIL((ENS.SKILSMISSENR));                             
         END;                                                                   
 /*OPPHØR_SAMBO ER NYTT - HL*/                                                  
      IF ENS.OPPHØR_SAMBONR = 0 THEN                                            
         S001UE1O.OPPHØR_SAMBOA = NORMAL;                                       
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.OPPHØR_SAMBOA = DFHBMBRY;                                  
