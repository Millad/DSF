 /* *********************************************************** */              
                                                                                
 OVERFØR_EN_SPES_MAP:                                                           
   PROC;                                                                        
   DCL                                                                          
      NORMAL  CHAR (1) INIT(' '),                                               
      NOR_NUM CHAR (1) INIT('&'),                                               
      BRI_NUM CHAR (1) INIT('Q');                                               
   DCL SYV_TIL_FEM     CHAR (7);                                                
   DCL FEM DEF SYV_TIL_FEM POS(3) CHAR (5);                                     
                                                                                
  /*  IF EN_SPES.STATSBORGER   ^= 0     THEN */                                 
         S001UE1O.STATSBORGERO =    EN_SPES.STATSBORGER ;                       
                                                                                
  /*  IF EN_SPES.BOSTED   ^=  0    THEN           */                            
         S001UE1O.BOSTEDO = EN_SPES.BOSTED ;                                    
                                                                                
      IF F_FELT_PIC_CHAR7(EN_SPES.TP_NORDISK ) ^= '0000000' THEN                
         S001UE1O.NORDISK_TPO =                                                 
                       F_FELT_PIC_CHAR7(EN_SPES.TP_NORDISK);                    
                                                                                
                                                                                
      IF F_FELT_PIC_CHAR3(EN_SPES.TP_NORDISK_LAND ) ^= '000' THEN               
         S001UE1O.NORDISK_LANDO =                                               
                         F_FELT_PIC_CHAR3(EN_SPES.TP_NORDISK_LAND);             
                                                                                
      IF F_FELT_PIC_CHAR7(EN_SPES.PENSJON_EØS ) ^= '0000000' THEN               
         S001UE1O.EØS_PENSJONO =                                                
                       F_FELT_PIC_CHAR7(EN_SPES.PENSJON_EØS);                   
                                                                                
         S001UE1O.INNT_PRØVET_EØS_PENSO = EN_SPES.INNT_PRØVET_EØS_PENS ;        
         S001UE1O.TT_EØS_ANT_ÅRO = EN_SPES.TT_EØS_ANT_ÅR           ;            
         S001UE1O.TT_EØS_ANT_MNDO = EN_SPES.TT_EØS_ANT_MND         ;            
                                                                                
                                                                                
 /*  MARKERING AV ATTRIBUTTER   */                                              
