            S001UE1I.SIVILSTANDL = CURSOR_POS;                                  
            IF ENS.SIVILSTANDNR ^= 999 THEN                                     
               CALL SKRIV_FEIL((ENS.SIVILSTANDNR));                             
         END;                                                                   
                                                                                
                                                                                
      IF ENS.FLYTTET_FRANR = 0 THEN                                             
         S001UE1O.FLYTTET_FRAA = NORMAL;                                        
      ELSE                                                                      
         DO;                                                                    
           S001UE1O.FLYTTET_FRAA = DFHBMBRY;                                    
