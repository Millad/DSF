      ELSE                                                                      
         DO;                                                                    
            S001UE1O.NORDISK_TPA = BRI_NUM;                                     
            S001UE1I.NORDISK_TPL = CURSOR_POS;                                  
            IF EN_SPES.TP_NORDISKNR ^= 999 THEN                                 
               CALL SKRIV_FEIL((EN_SPES.TP_NORDISKNR));                         
         END;                                                                   
                                                                                
      IF EN_SPES.TP_NORDISK_LANDNR = 0 THEN                                     
         S001UE1O.NORDISK_LANDA = NOR_NUM;                                      
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.NORDISK_LANDA = BRI_NUM;                                   
            S001UE1I.NORDISK_LANDL = CURSOR_POS;                                
            IF EN_SPES.TP_NORDISK_LANDNR ^= 999 THEN                            
               CALL SKRIV_FEIL((EN_SPES.TP_NORDISK_LANDNR));                    
         END;                                                                   
                                                                                
      IF EN_SPES.PENSJON_EØSNR = 0 THEN                                         
         S001UE1O.EØS_PENSJONA = NOR_NUM;                                       
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.EØS_PENSJONA = BRI_NUM;                                    
            S001UE1I.EØS_PENSJONL = CURSOR_POS;                                 
            IF EN_SPES.PENSJON_EØSNR ^= 999 THEN                                
               CALL SKRIV_FEIL((EN_SPES.PENSJON_EØSNR));                        
         END;                                                                   
                                                                                
                                                                                
      IF EN_SPES.INNT_PRØVET_EØS_PENSNR = 0 THEN                                
         S001UE1O.INNT_PRØVET_EØS_PENSA = NORMAL ;                              
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.INNT_PRØVET_EØS_PENSA = DFHBMBRY;                          
            S001UE1I.INNT_PRØVET_EØS_PENSL = CURSOR_POS;                        
            IF EN_SPES.INNT_PRØVET_EØS_PENSNR ^= 999 THEN                       
               CALL SKRIV_FEIL((EN_SPES.INNT_PRØVET_EØS_PENSNR));               
         END;                                                                   
                                                                                
      IF EN_SPES.TT_EØS_ANT_ÅRNR = 0 THEN                                       
         S001UE1O.TT_EØS_ANT_ÅRA = NOR_NUM;                                     
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.TT_EØS_ANT_ÅRA = BRI_NUM;                                  
            S001UE1I.TT_EØS_ANT_ÅRL = CURSOR_POS;                               
