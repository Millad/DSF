 /*   MAPSET:                                                   */              
 /*                                                             */              
 /* *********************************************************** */              
                                                                                
 OVERFØR_EN_INNTEKT_MAP:                                                        
   PROC;                                                                        
   DCL                                                                          
      NORMAL  CHAR (1) INIT(' '),                                               
      NOR_NUM CHAR (1) INIT('&'),                                               
      BRI_NUM CHAR (1) INIT('Q');                                               
                                                                                
         S001UE1O.PI_SØKERO =                                                   
                     F_FELT_PIC_CHAR6((EN_INNTEKT.PENSJONSINNT_SØKER));         
         S001UE1O.PI_EKO =                                                      
                     F_FELT_PIC_CHAR6((EN_INNTEKT.PENSJONSINNT_EK));            
         S001UE1O.AI_SØKERO =                                                   
                     F_FELT_PIC_CHAR7((EN_INNTEKT.ARBEIDSINNT_SØKER));          
                                                                                
         S001UE1O.AI_EKO =                                                      
                        F_FELT_PIC_CHAR7((EN_INNTEKT.ARBEIDSINNT_EK));          
                                                                                
         S001UE1O.OVER4GO =      EN_INNTEKT.MINST_4G;                           
     /*                                                                         
      IF EN_INNTEKT.MINST_4G   = 'N' THEN                                       
         S001UE1O.UNDER4GO =  'X';                                              
      ELSE                                                                      
      IF EN_INNTEKT.MINST_4G   = 'J' THEN                                       
         S001UE1O.OVER4GO =  'X';                                               
                                                                                
    */                                                                          
 /*  MARKERING AV ATTRIBUTTER   */                                              
      IF EN_INNTEKT.ARBEIDSINNT_SØKERNR = 0 THEN                                
         S001UE1O.AI_SØKERA = NOR_NUM;                                          
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.AI_SØKERA = BRI_NUM;                                       
            S001UE1I.AI_SØKERL = CURSOR_POS;                                    
            IF EN_INNTEKT.ARBEIDSINNT_SØKERNR ^= 999 THEN                       
               CALL SKRIV_FEIL((EN_INNTEKT.ARBEIDSINNT_SØKERNR));               
         END;                                                                   
                                                                                
      IF EN_INNTEKT.ARBEIDSINNT_EKNR = 0 THEN                                   
         S001UE1O.AI_EKA = NOR_NUM;                                             
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.AI_EKA = BRI_NUM;                                          
            S001UE1I.AI_EKL = CURSOR_POS;                                       
            IF EN_INNTEKT.ARBEIDSINNT_EKNR ^= 999 THEN                          
               CALL SKRIV_FEIL((EN_INNTEKT.ARBEIDSINNT_EKNR));                  
         END;                                                                   
                                                                                
                                                                                
      IF EN_INNTEKT.PENSJONSINNT_SØKERNR = 0 THEN                               
         S001UE1O.PI_SØKERA = NOR_NUM;                                          
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.PI_SØKERA = BRI_NUM;                                       
            S001UE1I.PI_SØKERL = CURSOR_POS;                                    
            IF EN_INNTEKT.PENSJONSINNT_SØKERNR ^= 999 THEN                      
               CALL SKRIV_FEIL((EN_INNTEKT.PENSJONSINNT_SØKERNR));              
         END;                                                                   
                                                                                
      IF EN_INNTEKT.PENSJONSINNT_EKNR = 0 THEN                                  
         S001UE1O.PI_EKA = NOR_NUM;                                             
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.PI_EKA = BRI_NUM;                                          
            S001UE1I.PI_EKL = CURSOR_POS;                                       
            IF EN_INNTEKT.PENSJONSINNT_EKNR ^= 999 THEN                         
               CALL SKRIV_FEIL((EN_INNTEKT.PENSJONSINNT_EKNR));                 
         END;                                                                   
                                                                                
   /*  ENDRET AV SATISH 11.02.05                                                
      IF EN_INNTEKT.MINST_4GNR = 0  THEN                                        
         DO;                                                                    
            S001UE1O.UNDER4GA = NOR_NUM;                                        
            S001UE1O.OVER4GA = NOR_NUM;                                         
         END;                                                                   
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.UNDER4GA = BRI_NUM;                                        
            S001UE1I.UNDER4GL = CURSOR_POS;                                     
            IF EN_INNTEKT.MINST_4GNR ^= 999 THEN                                
               CALL SKRIV_FEIL((EN_INNTEKT.MINST_4GNR));                        
         END;                                                                   
   /* **********************************   */                                   
      IF EN_INNTEKT.MINST_4GNR = 0  THEN                                        
            S001UE1O.OVER4GA = NOR_NUM;                                         
      ELSE                                                                      
         DO;                                                                    
            S001UE1O.OVER4GA = BRI_NUM;                                         
            S001UE1I.OVER4GL = CURSOR_POS;                                      
            IF EN_INNTEKT.MINST_4GNR ^= 999 THEN                                
               CALL SKRIV_FEIL((EN_INNTEKT.MINST_4GNR));                        
         END;                                                                   
    /*  NYTT ********************************* */                               
                                                                                
      IF FRA_UTEN_DIALOG THEN       /* BLOKKER ALT SAMMEN   */                  
         DO;                                                                    
           S001UE1O.OVER4GA     = DFHBMASK;                                     
           S001UE1O.PI_SØKERA   = DFHBMASK;                                     
           S001UE1O.PI_EKA      = DFHBMASK;                                     
           S001UE1O.AI_SØKERA   = DFHBMASK;                                     
           S001UE1O.AI_EKA      = DFHBMASK;                                     
         END;                                                                   
   END OVERFØR_EN_INNTEKT_MAP;                                                  
                                                                                
 /* *********************************************************** */              
