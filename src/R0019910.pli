 F_NUMERISK:                                                            00000160
   PROC(NUMFELT) RETURNS (BIT(1));                                      00000170
       DCL  NUMFELT CHAR(*),                                            00000180
            RETUR BIT(1) INIT('1'B);                                    00000190
            IF VERIFY(NUMFELT,'0123456789') > 0 THEN                    00000200
            RETUR = '0'B;                                               00000210
            RETURN(RETUR);                                              00000220
 END F_NUMERISK;                                                        00000230
