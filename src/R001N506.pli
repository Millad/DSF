               S001N51O.FNR_BARN1A = NOR_NUM;                                   
            ELSE                                                                
               DO;                                                              
                  S001N51O.FNR_BARN1A = BRI_NUM;                                
                  S001N51I.FNR_BARN1L = CURSOR_POS;                             
                  IF A1B.FNR_BARNNR(ANT_BARN) ^= 999 THEN                       
                    CALL SKRIV_FEIL(A1B.FNR_BARNNR(ANT_BARN));                  
               END;                                                             
            ANT_BARN = ANT_BARN +1;                                             
         END;                                                                   
      IF F_FELT_PIC_CHAR11(A1B.FNR_BARN(ANT_BARN)) ^= (11)'0' THEN              
