           EXEC CICS WRITE DATASET ('F0019H21')                                 
                       FROM (TRKLISTE )                                         
                       RBA                                                      
                       RIDFLD (RBA_PEKER);                                      
           S0019HO.ANT_TIL_TRKLISTO =                                           
                      S0019HO.ANT_TIL_TRKLISTO + 1;                             
