           TRKLISTE.B02_DATA.UP   = B02.UFØRPENS.PÅ(SØKER_IND);                 
           TRKLISTE.B02_DATA.YP   = B02.YRKEPENS.PÅ(SØKER_IND);                 
           TRKLISTE.B02_DATA.AP   = B02.ALDERSP.PÅ(SØKER_IND);                  
           TRKLISTE.B02_DATA.EP   = B02.ETTEPENS.PÅ_AVD(SØKER_IND);*/           
           TRKLISTE.B01_DATA.FORS_TILL =  0;                                    
           TRKLISTE.B02_DATA.FORS_TILL =  0;                                    
   /*      TRKLISTE.B01_DATA.BUP = 0;                                           
           TRKLISTE.B02_DATA.BUP = 0;    */                                     
 /*        TRKLISTE.B01_DATA.BT_F      =                                        
                         B01.UFØRPENS.BT(SØKER_IND);                            
           TRKLISTE.B02_DATA.BT_F      =                                        
                         B02.UFØRPENS.BT(SØKER_IND);                            
           TRKLISTE.B01_DATA.BT_S      =                                        
                         B01.BT_SÆRBARN(SØKER_IND);                             
           TRKLISTE.B02_DATA.BT_F      =                                        
                         B02.BT_SÆRBARN(SØKER_IND);       */                    
        /* TRKLISTE.B02_DATA.BUP  = B02.BUP(SØKER_IND,1);     */                
           TRKLISTE.YTELSE_FØR    = B01.SUM_YTELSE(SØKER_IND);                  
           TRKLISTE.YTELSE_ETTER  = B02.SUM_YTELSE(SØKER_IND);                  
       /*  TRKLISTE.SPT_FØR       = B01.UFØRPENS.SPT(SØKER_IND);                
           TRKLISTE.SPT_ETTER     = B02.UFØRPENS.SPT(SØKER_IND);                
