 R00119:                                                                        
   PROC(COMMAREA_PEKER) OPTIONS(MAIN);                                          
 %PAGE;                                                                         
      %INCLUDE S00119;                 /* AFP-MAPSETTET   */                    
 %PAGE;                                                                         
      %INCLUDE P0011901;               /* TRANS-OMR FOR AFP          */         
 %PAGE;                                                                         
      %INCLUDE P0019906;               /* TRANS_OPPL_OMR (BASED) */             
 %PAGE;                                                                         
      %INCLUDE P0019908;               /* KOM_OMRÅDE (BASED) */                 
 %PAGE;                                                                         
      %INCLUDE P0019910;               /* STYRINGS_OMRÅDE (BASED) */            
 %PAGE;                                                                         
      %INCLUDE P0019912;               /*  DIV_OMRÅDE (BASED)      */           
 %PAGE;                                                                         
      %INCLUDE DFHBMSCA;                                                        
 %PAGE;                                                                         
   DCL                                                                          
      (BMSMAPBR,                                                                
      COMMAREA_PEKER) PTR;                                                      
   DCL                                                                          
      (CSTG,                                                                    
      ADDR,                                                                     
      VERIFY,                                                                   
      UNSPEC,                                                                   
      ONCODE,                                                                   
      SUBSTR) BUILTIN;                                                          
   DCL                                                                          
      (FEIL_FUNNET,FEIL19,FEIL20) BIT(1),                                       
      FEIL_I_SØKER                BIT(1),                                       
      FEIL_I_SPES                 BIT(1),                                       
      ONKODE                      PIC'9999',                                    
      CURSOR_POS                  FIXED BIN(15) INIT(-1),                       
      ONK DEF ONKODE              CHAR(4),                                      
      FEILKODE                    CHAR(4),                                      
      DSNAVN                      CHAR(8),                                      
      ANT_FEIL_SKREVET            FIXED DEC (3);                                
 %PAGE;                                                                         
      ON ERROR SNAP                                                             
      BEGIN;                                                                    
                                                                                
         ON ERROR SYSTEM        ;                                               
         ONKODE=ONCODE          ;                                               
         FEILKODE = ONK         ;                                               
         DSNAVN   = EIBDS       ;                                               
         GO TO FEILBEH          ;                                               
      END;                                                                      
                                                                                
                                                                                
 FEILKODE  = 'FEIL';                                                            
 DSNAVN    = '        ';                                                        
                                                                                
 EXEC CICS HANDLE AID PF1(PF1) PF2(PF2);                                        
                                                                                
 EXEC CICS HANDLE CONDITION ERROR(FEILBEH);                                     
                                                                                
                                                                                
 KOM_OMR.PEKER_LISTE.DIV_PARAM_PEKER  = ADDR(KOM_OMR.DIV_PARAM_OMR);            
                                                                                
 IF FRA_CICS THEN                                                               
  DO;                                                                           
    KOM_OMR.PEKER_LISTE.TRANS_OPPL_PEKER = ADDR(KOM_OMR.TRANS_OPPL_OMR);        
    KOM_OMR.PEKER_LISTE.TRANS_PEKER      = ADDR(KOM_OMR.TRANS_OMR);             
    KOM_OMR.PEKER_LISTE.STYRINGS_PEKER   = ADDR(KOM_OMR.STYRINGS_OMR);          
    ATK_KOM_PTR           = ADDR  (KOM_OMR.ATK_COM_OMR  ) ;                     
  END;                                                                          
                                                                                
                                                                                
 IF HENT_FRAM_MAP  THEN                                                         
   DO;                                                                          
     EXEC CICS SEND MAP('S001191') MAPSET('S001193') MAPONLY                    
     SET (BMSMAPBR);                                                            
     ALLOCATE S001191O;                                                         
     S001191O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN;                             
   END;                                                                         
                                                                                
 RECEIVE_MAP:                                                                   
                                                                                
                                                                                
 IF ^HENT_FRAM_MAP THEN                                                         
    DO;                                                                         
       EXEC CICS RECEIVE MAP('S001191')                                         
                 MAPSET('S001193') SET(BMSMAPBR);                               
       CALL BLANK_AFP_MELDNR;                            /* R0011902 */         
       CALL MAP_SPESIAL_INFO_PROT;                                              
       FEIL_MELD_NR = 0;                                                        
    END;                                                                        
                                                                                
 IF FUNKSJONSKODEL > 0 THEN                                                     
    DO;                                                                         
       FUNKSJONSKODE = FUNKSJONSKODEI;                                          
       EXEC CICS XCTL PROGRAM('R0010301') COMMAREA(KOM_OMR);                    
    END;                                                                        
                                                                                
 IF STYRE_KODEL > 0 THEN                                                        
    DO;                                                                         
       STYREKODE = STYRE_KODEI;                                                 
       TRANSKODE = 'R041';                                                      
       EXEC CICS XCTL PROGRAM('R0010401') COMMAREA(KOM_OMR);                    
    END;                                                                        
                                                                                
 IF STYREKODE ^= 'AF'  &  FRA_CICS THEN      /*   CHANGE TO */                  
    TRANSKODE = 'R041';                                                         
                                                                                
 ELSE                                                                           
                                                                                
  DO;                                                                           
   ANT_FEIL_SKREVET = 0;                                                        
   FEIL_FUNNET      = '0'B;                                                     
   FEIL_I_SØKER     = '0'B;                                                     
   FEIL_I_SPES      = '0'B;                                                     
   TRANS_RETURKODE  = TRANSKODE;                                                
                                                                                
            SELECT (TRANSKODE);                                                 
               WHEN('R190')                                                     
                  DO;                                                           
                     CALL BLANK_AFP_SØKER;                /* R0011902 */        
                     CALL BLANK_AFP_SPES;                 /* R0011902 */        
                     CALL BLANK_AFP_MELDNR;               /* R0011902 */        
                     CALL OVERFØR_AFP_SØKER;              /* R0011909 */        
                     CALL KONTROLL_AFP_SØKER(FEIL_I_SØKER);/*R0011903*/         
                  END;                                                          
               WHEN('R191')                                                     
                  DO;                                                           
                     CALL BLANK_AFP_MELDNR;               /* R0011902 */        
                     IF FUNKSJONSKODE ^= 'V' THEN                               
                        DO;                                                     
                           CALL BLANK_AFP_SPES;           /* R0011902 */        
                        END;                                                    
                     CALL OVERFØR_AFP_SØKER;              /* R0011909 */        
                     CALL KONTROLL_AFP_SØKER(FEIL_I_SØKER);/*R0011903*/         
                  END;                                                          
               WHEN('R192')                                                     
                  DO;                                                           
                     CALL BLANK_AFP_MELDNR;               /* R0011902 */        
                     CALL BLANK_AFP_SPES;                 /* R0011902 */        
                     CALL OVERFØR_AFP_SØKER;              /* R0011909 */        
                     CALL KONTROLL_AFP_SØKER(FEIL_I_SØKER);/*R0011903*/         
                  END;                                                          
               WHEN('R193' , 'R194' )                                           
                  DO;                                                           
                     CALL BLANK_AFP_MELDNR;               /* R0011902 */        
                     CALL OVERFØR_AFP_SØKER;              /* R0011909 */        
                     CALL OVERFØR_AFP_SPES;               /* R0011909 */        
                     CALL KONTROLL_AFP_SØKER(FEIL_I_SØKER);/*R0011903*/         
                     IF AFP.NORSK_BOSATT = 'J' THEN                             
                        CALL BLANK_AFP_SPES;              /* R0011902 */        
                     ELSE                                                       
                        CALL KONTROLL_AFP_SPES(FEIL_I_SPES);/*R0011905*/        
                                                                                
                  END;                                                          
               OTHERWISE                                                        
                  ;                                                             
 /* INGEN UTGANG DA CICS IKKE GODTAR UGYLDIGE TRANSAKSJONSKODER */              
            END;                                                                
                                                                                
            IF FEIL_I_SØKER = '0'B &                                            
               FEIL_I_SPES  = '0'B THEN                                         
               FEIL_FUNNET = '0'B;                                              
            ELSE                                                                
               FEIL_FUNNET = '1'B;                                              
                                                                                
                                                                                
               IF AFP.NORSK_BOSATT = 'N' & ^FEIL_FUNNET THEN                    
                  TRANSKODE        = 'R193';                                    
                                                                                
            IF ( TRANSKODE = 'R192' !                                           
             TRANSKODE = 'R193' ! TRANSKODE =  'R194') &                        
             AFP.NORSK_BOSATT = 'J' THEN                                        
               TRANSKODE = 'R191';                                              
                                                                                
            ELSE                                                                
            IF (TRANSKODE = 'R193' ! TRANSKODE = 'R194') &                      
                AFP.NORSK_BOSATT ='J'  THEN                                     
              TRANSKODE = 'R192';                                               
                                                                                
            ELSE                                                                
            IF (TRANSKODE = 'R192' ! TRANSKODE = 'R194') &                      
               AFP.NORSK_BOSATT = 'N'   THEN                                    
                TRANSKODE = 'R193';                                             
                                                                                
            ELSE                                                                
            IF (TRANSKODE = 'R192' ! TRANSKODE = 'R193') &                      
                AFP.NORSK_BOSATT ='N'         THEN                              
                TRANSKODE = 'R194';                                             
                                                                                
            IF FEIL_FUNNET ! HENT_FRAM_MAP ! FRA_UTEN_DIALOG !                  
            TRANS_RETURKODE ^= TRANSKODE THEN                                   
                                                                                
               DO;                                                              
                  IF TRANSKODE = 'R190' THEN                                    
                     TRANSKODE = 'R191';                                        
                  CALL BLANK_AFP_MAP;                     /* R0011909 */        
                  CALL OVERFØR_AFPSØKER_MAP;             /* R0011906 */         
                  SELECT (TRANSKODE);                                           
                     WHEN ('R193' , 'R194')                                     
                        DO;                                                     
                           CALL OVERFØR_AFPSPES_MAP;      /* R0011906 */        
                        END;                                                    
                     OTHERWISE                                                  
                        ;                                                       
                  END;                                                          
                                                                                
                IF HENT_FRAM_MAP THEN                                           
                   EXEC CICS SEND MAP('S001191') MAPSET('S001193')              
                                                        ERASE CURSOR;           
                ELSE                                                            
                   EXEC CICS SEND MAP('S001191') MAPSET('S001193')              
                                            DATAONLY ERASEAUP CURSOR;           
                FEIL_MELD_NR  = 666;/* => FEIL_FUNNET ER SATT. */               
                HENT_FRAM_MAP = '0'B;                                           
                                                                                
                IF  FRA_CICS THEN                                               
                    EXEC CICS RETURN TRANSID(TRANSKODE)                         
                                                  COMMAREA(KOM_OMR);            
                ELSE                                                            
                    GO TO RECEIVE_MAP;                                          
               END;                                                             
                                                                                
            ELSE                                                                
              DO;                                                               
                                                                                
                IF TRANSKODE = 'R190' THEN                                      
                  TRANS_RETURKODE = 'R191';                                     
                ELSE                                                            
                  TRANS_RETURKODE = TRANSKODE;                                  
                IF  FRA_CICS  THEN                                              
                  DO;                                                           
                    BLANKETTYPE = STYREKODE;                                    
                    TRANSKODE = 'R042';                                         
                  END;                                                          
              END;                                                              
                                                                                
  END;                                                                          
                                                                                
  CALL UTGANG;                                                                  
                                                                                
  UTGANG: PROC;                                                                 
    IF  FRA_CICS THEN                                                           
      EXEC CICS XCTL PROGRAM('R0010401') COMMAREA(KOM_OMR);                     
    ELSE                                                                        
      EXEC CICS RETURN;                                                         
  END UTGANG;                                                                   
                                                                                
                                                                                
  FEILBEH:                                                                      
                                                                                
     EXEC CICS HANDLE CONDITION ERROR(ABEND);                                   
     IF FEIL_MELD_NR = 0 THEN /* AB 01.08.16 */                                 
        PROGRAM_ID = 'R0011901';                                                
                                                                                
     S001191O.MELDING2O =                                                       
              'F E I L  H A R  O P P S T Å T T ! ! !.';                         
                                                                                
     S001191O.MELDING3O =                                                       
              'EIBRCODE : ' !! UNSPEC(EIBRCODE);                                
                                                                                
     S001191O.MELDING4O =                                                       
       'EIBFN    : ' !! UNSPEC(EIBFN) !! '. FEILKODE : ' !! FEILKODE !!         
                                            '. DATASETT : ' !! DSNAVN;          
                                                                                
     S001191O.MELDING5O =                                                       
                   'PROGRAMNAVN :  ' !! PROGRAM_ID    !!                        
                   '.INGEN OPPDATERING ER BLITT UTFØRT.';                       
                                                                                
     S001191O.MELDING6O =                                                       
                   'TA UTSKRIFT!!!. TRYKK DERETTER "ENTER".';                   
                                                                                
                                                                                
     EXEC CICS SEND MAP('S001191')                                              
                                                 MAPSET('S001193');             
                                                                                
     EXEC CICS RECEIVE MAP('S001191')                                           
                                    MAPSET('S001193') SET(BMSMAPBR);            
                                                                                
     CALL MAP_SPESIAL_INFO_PROT;                                                
                                                                                
     EXEC CICS SYNCPOINT ROLLBACK;                                              
                                                                                
                                                                                
     TERMINERINGS_IND = 'F';                                                    
                                                                                
     TRANSKODE  = 'R041';                                                       
                                                                                
     CALL UTGANG;                                                               
                                                                                
  ABEND:                                                                        
     EXEC CICS ABEND ABCODE(FEILKODE);                                          
                                                                                
  PF1:                                                                          
                                                                                
 /* LEGGER UNDERKJENT TRANS UT PÅ VENTEDATABASE HVIS GYLDIG     */              
 /* FNR OG MIRKNINGSDATO,ELLERS FORKASTES TRANSAKSJONEN.        */              
                                                                                
     IF ^(AFP.FNRNR = 1          !                                              
          AFP.FNRNR = 200        !                                              
          AFP.VIRK_DATO_ÅMDNR = 9 !                                             
          AFP.VIRK_DATO_ÅMDNR = 200) THEN                                       
        DO;                                                                     
                                                                                
           IF FØDSNUMMER    = 0   THEN                                          
                                                                                
 /* SKRIVER MELDING OM AT TRANSAKSJONEN ER FORKASTET.           */              
                                                                                
              DO;                                                               
                 FEIL_MELD_NR  = 0;                                             
                 S001191O.MELDING6O =                                           
                         'REGISTERINGEN ER IKKE AVSLUTTET MED "ENTER"'!!        
                         '.TRANSAKSJONEN ER FORKASTET.';                        
              END;                                                              
           ELSE                                                                 
              DO;                                                               
                                                                                
                 SEARCH_FNR  = TRANS_OPPL_OMR.FØDSNUMMER;                       
                 IF FRA_CICS THEN                                               
                    BLANKETTYPE = STYREKODE;                                    
                                                                                
                                                                                
                 IF FEIL_MELD_NR = 666 &      /*BLANKETTKONTROLLFEIL*/          
                    ^FRA_MED_DIALOG     THEN  /*IKKE BASEKONTROLLFEIL*/         
                    FUNKSJONSKODE = 'B';                                        
                                                                                
                 CALL BLANK_AFP_MELDNR;                  /* R0011902 */         
                                                                                
                                                                                
                 IF AFP.NORSK_BOSATT = 'N' THEN                                 
                    CALL OVERFØR_AFPSPES_MAP;            /* R0011906 */         
                                                                                
                 EXEC CICS LINK PROGRAM('R0010490') COMMAREA(KOM_OMR);          
                                                                                
 /* SKRIVER MELDING OM AT TRANSAKSJONEN ER LAGT UT PÅ VENTEBASE.*/              
                                                                                
                 S001191O.MELDING6O =                                           
                         'TRANSAKSJONEN ER LAGT UT PÅ VENTEREGISTER.';          
              END;                                                              
                                                                                
                                                                                
           EXEC CICS SEND MAP('S001191') MAPSET('S001193')                      
                                                           DATAONLY;            
     /* DUMMY SEND-KALL.                              */                        
                                                                                
           EXEC CICS SEND MAP('S001191')                                        
                                                  MAPSET('S001193');            
           EXEC CICS DELAY INTERVAL(005);                                       
                                                                                
           TERMINERINGS_IND = 'F';                                              
                                                                                
           TRANSKODE  = 'R041';                                                 
                                                                                
           CALL UTGANG;                                                         
                                                                                
        END;                                                                    
     ELSE                                                                       
        DO;                                                                     
           TERMINERINGS_IND = 'F';                                              
                                                                                
           TRANSKODE = 'R041';                                                  
                                                                                
           S001191O.MELDING6O =                                                 
                                                                                
   'NB** TRANSAKSJONEN ER FORKASTET P.G.A. UGYLDIG FNR EL. VDATO **NB';         
                                                                                
           EXEC CICS SEND MAP('S001191') MAPSET('S001193')                      
                                                           DATAONLY;            
     /* DUMMY SEND-KALL.                            */                          
                                                                                
           EXEC CICS SEND MAP('S001191')                                        
                                    MAPSET('S001193');                          
                                                                                
           EXEC CICS DELAY INTERVAL(005);                                       
           CALL UTGANG;                                                         
        END;                                                                    
                                                                                
                                                                                
  PF2:                                                                          
     TERMINERINGS_IND   = 'F';                                                  
                                                                                
     S001191O.MELDING6O =                                                       
                                                                                
      'NBNBNBNBNB***    TRANSAKSJONEN ER FORKASTET.    ***NBNBNBNBNB';          
                                                                                
     EXEC CICS SEND MAP('S001191') MAPSET('S001193')                            
                                                      DATAONLY ;                
     /* DUMMY SEND-KALL.                                     */                 
                                                                                
     EXEC CICS SEND MAP('S001191')                                              
                                    MAPSET('S001193');                          
     EXEC CICS DELAY INTERVAL(005);                                             
                                                                                
                                                                                
     TRANSKODE = 'R041';                                                        
     CALL UTGANG;                                                               
                                                                                
                                                                                
  MAP_SPESIAL_INFO_PROT:                                                        
               PROC;                                                            
     /*----------------------------------------------*/                         
     /* SPESIAL OPPLYSNINGER FELTER ER BLOKET        */                         
     /*----------------------------------------------*/                         
     /*----------------------------------------------*/                         
      S001191O.TT_F67A       =  DFHBMPRO;                                       
      S001191O.TT_E66_ÅA     =  DFHBMPRO;                                       
      S001191O.TT_E66_MA     =  DFHBMPRO;                                       
      S001191O.STATSBORGERA  =  DFHBMPRO;                                       
      S001191O.BOSATTA       =  DFHBMPRO;                                       
      S001191O.FRAVIK_FLYKTA =  DFHBMPRO;                                       
      S001191O.OVERKOMP_UTLA =  DFHBMPRO;                                       
      S001191O.FRAVIK_19_3A  =  DFHBMPRO;                                       
                                                                                
  END MAP_SPESIAL_INFO_PROT;                                                    
 %PAGE;                                                                         
