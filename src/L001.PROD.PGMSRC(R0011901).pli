 %PAGE;                                                                         
      %INCLUDE R0019964;               /* DAGENS DATO PLUSS 6 MDR   */          
      %INCLUDE R0019995;               /* KONV-FNR11-FNR13          */          
      %INCLUDE R0019945;               /* ATK_TILLGANG_KONTROLL   */            
      %INCLUDE R0019999;               /* KONTROLL_ACF2           */            
   END R00119;                                                                  
