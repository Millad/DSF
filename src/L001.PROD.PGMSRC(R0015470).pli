  /* ************************************************************** */          
  P_SKRIV_NEST_SISTE_STATUS : PROC;                                             
                                                                                
     /* *********************************************************** */          
     /*                                                             */          
     /*      SLUTT PÅ DEKLARASJONENE , EKSEKVERINGEN STARTER .      */          
     /*     ---------------------------------------------------     */          
     /*                                                             */          
     /*          INSERT STATUS - SEGM.                              */          
     /*                                                             */          
     /* *********************************************************** */          
                                                                                
     /* TRUDE. ENGANGSBELØP _11_7 KUN HJELPEFELT, SKAL IKKE SKRIVES  */         
     /* PÅ BASEN.    Y R K E S S K A D E                             */         
                                                                                
 /*  B00.STATUS.ENGANGSBELØP_11_7(W02_I) = 0    HL  */                          
                                                                                
      /* NY TILLEGGSTEST MARTIN JAN 2003 */                                     
      IF (DIV_PERIODE.AFP_SERVICE ^= 'J') THEN                                  
         DO; /* IKKE SERVICEBEREGNET AFP-PENSJON */                             
     W01_HJELPE_PTR                =   ADDR (B00.STATUS(W02_I));                
     SSA_UQUAL                     =  'STATUS   ';                              
     CALL  P_SKRIV_BARN_AV_ROT;                                                 
                                                                                
     IF  FEIL_MELD_NR              >   0                THEN                    
       DO;                                                                      
         /* ******************************************************* */          
         /* FEIL VED SKRIV AV  :         STATUS                     */          
         /* ******************************************************* */          
                                                                                
         GO TO RETUR;                                                           
       END;                                                                     
                                                                                
     /* *********************************************************** */          
     /*      INSERT TILKNYTTINGS-PERSONER                           */          
     /* *********************************************************** */          
                                                                                
     DO  I = 1  TO  13                                                          
         WHILE (B00.TILKN.FNR_TILKN(W02_I,I)  >  0);                            
                                                                                
       W01_HJELPE_PTR          =  ADDR (B00.TILKN(W02_I,I));                    
       SSA_UQUAL               = 'TILKN    ';                                   
       CALL P_SKRIV_BARN_AV_STATUS;                                             
                                                                                
       IF  FEIL_MELD_NR              >   0             THEN                     
         DO;                                                                    
           /* ***************************************************** */          
           /* FEIL VED SKRIV AV  :        TILKN                     */          
           /* ***************************************************** */          
                                                                                
           GO TO RETUR;                                                         
         END;                                                                   
     END;                                                                       
                                                                                
     /* *********************************************************** */          
     /*         INSERT FORSI SEGMENT                                */          
     /* *********************************************************** */          
                                                                                
     IF (B00.FORSI.FT_SKAL_BEREGNES(W02_I) ^= ' ' &                             
         B00.FORSI.FT_SKAL_BEREGNES(W02_I) ^= 'D' )  THEN                       
       DO;                                                                      
         W01_HJELPE_PTR        = ADDR(B00.FORSI(W02_I));                        
         SSA_UQUAL             = 'FORSI    ';                                   
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* *************************************************** */          
             /* FEIL VED SKRIV AV  :       FORSI                    */          
             /* *************************************************** */          
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
       END;                                                                     
     /* *********************************************************** */          
     /*         INSERT ALDERSPENSJONSOPPLYSNINGER                   */          
     /* *********************************************************** */          
                                                                                
  /* IF ((B00.PENSJONSTYPE1(W02_I)  = 'D')  &        200105 HL*/                
     IF  (B00.KONV_P_KODE(W02_I)    = 'K' !                                     
          B00.KONV_P_KODE(W02_I)    = 'O')      !                               
        (B00.UTTAKSDATO_ÅMD(W02_I)  > 0  )  !                                   
        (B00.PENSJONSTYPE1(W02_I)  =  'A')  !                                   
        (B00.PENSJONSTYPE1(W02_I)  =  'K')  THEN                                
       DO;                                                                      
         W01_HJELPE_PTR        = ADDR(B00.ALDERSP(W02_I));                      
         SSA_UQUAL             = 'ALDERSP  ';                                   
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* *************************************************** */          
             /* FEIL VED SKRIV AV  :       ALDERSP                  */          
             /* *************************************************** */          
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
                                                                                
         /* ******************************************************* */          
         /*               VENTEPERIODE                              */          
         /* ******************************************************* */          
                                                                                
         DO  I = 1  TO  5                                                       
             WHILE (B00.UTTAKSDATO_ÅMD_ØVRIGE(W02_I,I) > 0);                    
                                                                                
           W01_HJELPE_PTR    = ADDR (B00.VENTØVR(W02_I,I));                     
           SSA_UQUAL         = 'VENTØVR  ';                                     
           CALL  P_SKRIV_BARN_AV_STATUS;                                        
                                                                                
           IF   FEIL_MELD_NR              >   0             THEN                
             DO;                                                                
               /* ************************************************ */           
               /* FEIL VED SKRIV AV  :      VENTEØVR               */           
               /* ************************************************ */           
                                                                                
               GO TO RETUR;                                                     
             END;                                                               
         END;                                                                   
       END;              /* END AV ALDERSP SEGMENT  */                          
                                                                                
         /* ******************************************************* */          
         /*               AFPHIST                                   */          
         /* ******************************************************* */          
                                                                                
         END;/* IKKE SERVICEBEREGNET AFP-PENSJON */                             
                                                                                
         /* HER SIKRIVES AFP_HIST SEGMENTET OGSÅ VED SERVICEBEREGNING */        
         DO  I = 1  TO  30  /* ENDRET 20->30 SP 18.07.05 */                     
             WHILE (B00.AFPDATO_ÅMD(W02_I,I) > 0);                              
                                                                                
           W01_HJELPE_PTR    = ADDR (B00.AFPHIST(W02_I,I));                     
           SSA_UQUAL         = 'AFPHIST  ';                                     
           CALL  P_SKRIV_BARN_AV_STATUS;                                        
                                                                                
           IF   FEIL_MELD_NR              >   0             THEN                
             DO;                                                                
               /* ************************************************ */           
               /* FEIL VED SKRIV AV  :      VENTEØVR               */           
               /* ************************************************ */           
                                                                                
               GO TO RETUR;                                                     
             END;                                                               
         END;                                                                   
                                                                                
      IF (DIV_PERIODE.AFP_SERVICE ^= 'J') THEN                                  
         DO; /* IKKE SERVICEBEREGNET AFP-PENSJON */                             
                                                                                
     /* ********************************************************** */           
     /*           UFØREPENSJON                                     */           
     /* ********************************************************** */           
                                                                                
     IF B00.PENSJONSTYPE1(W02_I)  = 'U'    !                                    
        B00.PENSJONSTYPE1(W02_I)  = 'Y'    !                                    
           (B00.PENSJONSTYPE1(W02_I)  = 'D' &                                   
            B00.UFØRPENS.SPT_1291(W02_I) > 0  )  !                              
        B00.PENSJONSTYPE3(W02_I)  = 'Y'    !                                    
        B00.PENSJONSTYPE3(W02_I)  = 'U'            THEN                         
       DO;                                                                      
         W01_HJELPE_PTR        =   ADDR(B00.UFØRPENS(W02_I));                   
         SSA_UQUAL             =  'UFØRPENS ';                                  
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* ************************************************** */           
             /* FEIL VED SKRIV AV  :     UFØRPENS                  */           
             /* ************************************************** */           
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
       END;                                                                     
                                                                                
     /* ********************************************************** */           
     /*   LEGGER INN UFØRE-OPPLYSNINGER                            */           
     /* ********************************************************** */           
   /*0697 HL:*/                                                                 
     DO  I=1 TO 7               WHILE  (B00.UFT_ÅMD  (W02_I,I) > 0);            
       W01_HJELPE_PTR        =  ADDR   (B00.UFØRHIST (W02_I,I) );               
       SSA1_UFØRHIST.UFT_ÅMD =          B00.UFT_ÅMD  (W02_I,I);                 
       SSA_UQUAL             =                'UFØRHIST ';                      
       CALL  P_SKRIV_BARN_AV_STATUS;                                            
                                                                                
       IF  FEIL_MELD_NR      >          0               THEN                    
         DO;                                                                    
           /* ***************************************************** */          
           /*  FEIL VED SKRIV AV  :  UFØRHIST                       */          
           /* ***************************************************** */          
                                                                                
           GO TO  RETUR;                                                        
         END;                                                                   
       /* ********************************************************* */          
       /*  LEGGER INN UFØREGRADPERIODER INNENFOR                    */          
       /*  UFØREHISTORIKK-PERIODER .                                */          
       /* ********************************************************* */          
                                                                                
   /*0697 HL: + JFA 6.4.04 */                                                   
       DO J = 1 TO HBOUND(B00.UFØRGRAD,3)                                       
  /*   DO  J=1 TO 7 */       WHILE (B00.UFG_DATO_ÅMD (W02_I,I,J) > 0);          
         W01_HJELPE_PTR   =   ADDR (B00.UFØRGRAD    (W02_I,I,J));               
         SSA_UQUAL        =               'UFØRGRAD ';                          
         CALL P_SKRIV_BARN_AV_UFØRHIST;                                         
                                                                                
         IF  FEIL_MELD_NR      >          0               THEN                  
           DO;                                                                  
             /* *************************************************** */          
             /*  FEIL VED SKRIV AV  :  UFØRGRAD                     */          
             /* *************************************************** */          
                                                                                
             GO TO  RETUR;                                                      
           END;                                                                 
       END;                                                                     
     END;                                                                       
                                                                                
       /* ******************************************************** */           
       /*         YRKESSKADE                                       */           
       /* ******************************************************** */           
                                                                                
       IF B00.PENSJONSTYPE1(W02_I) = 'Y'   !                                    
          B00.PENSJONSTYPE3(W02_I) = 'Y'   !                                    
          (B00.PENSJONSTYPE1(W02_I) = 'A'   &                                   
           B00.KONV_P_KODE  (W02_I) = 'Y')         THEN                         
         DO;                                                                    
           W01_HJELPE_PTR      =   ADDR(B00.YRKEPENS(W02_I));                   
           SSA_UQUAL           =  'YRKEPENS ';                                  
           CALL P_SKRIV_BARN_AV_STATUS;                                         
                                                                                
           IF FEIL_MELD_NR             >   0             THEN                   
             DO;                                                                
               /* ************************************************ */           
               /* FEIL VED SKRIV AV :    YRKEPENS                  */           
               /* ************************************************ */           
                                                                                
               GO TO RETUR;                                                     
             END;                                                               
         END;                                                                   
                                                                                
               /* ************************************************ */           
               /* YRKEPENS SKAL SKRIVES UT FOR DØDE                */           
               /* ************************************************ */           
                                                                                
       IF B00.PENSJONSTYPE1(W02_I) = 'D'  &                                     
          B00.YRKEPENS.YUG (W02_I) > 0          THEN                            
         DO;                                                                    
           W01_HJELPE_PTR      =   ADDR(B00.YRKEPENS(W02_I));                   
           SSA_UQUAL           =  'YRKEPENS ';                                  
           CALL P_SKRIV_BARN_AV_STATUS;                                         
                                                                                
           IF FEIL_MELD_NR             >   0             THEN                   
             DO;                                                                
               /* ************************************************ */           
               /* FEIL VED SKRIV AV :    YRKEPENS                  */           
               /* ************************************************ */           
                                                                                
               GO TO RETUR;                                                     
             END;                                                               
         END;                                                                   
                                                                                
       /* ********************************************************** */         
       /* LEGGER INN YRKEHIST-OPPLYSNINGER                           */         
       /* ********************************************************** */         
                                                                                
       DO I = 1 TO 10    WHILE  (B00.YUFT_ÅMD (W02_I,I) > 0);                   
         W01_HJELPE_PTR      =  ADDR   (B00.YRKEHIST (W02_I,I) );               
         SSA1_YRKEHIST.YUFT_ÅMD =       B00.YUFT_ÅMD (W02_I,I);                 
         SSA_UQUAL           =                'YRKEHIST ';                      
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF FEIL_MELD_NR     >          0               THEN                    
           DO;                                                                  
             /* *************************************************** */          
             /* FEIL VED SKRIV AV :  YRKEHIST                       */          
             /* *************************************************** */          
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
         /* ******************************************************* */          
         /* LEGGER INN YRKEGRADPERIODER INNENFOR                    */          
         /* YRKEHISTORIKK-PERIODER .                                */          
         /* ******************************************************* */          
                                                                                
         DO J = 1 TO 5   WHILE (B00.YUG_DATO_ÅMD (W02_I,I,J) > 0);              
           W01_HJELPE_PTR =   ADDR (B00.YRKEGRAD    (W02_I,I,J));               
           SSA_UQUAL      =               'YRKEGRAD ';                          
           CALL P_SKRIV_BARN_AV_YRKEHIST;                                       
                                                                                
           IF FEIL_MELD_NR     >          0               THEN                  
             DO;                                                                
               /* ************************************************* */          
               /* FEIL VED SKRIV AV :  YRKEGRAD                     */          
               /* ************************************************* */          
                                                                                
               GO TO RETUR;                                                     
             END;                                                               
         END;                                                                   
       END;                                                                     
                                                                                
     /* ***************************************************** */                
     /*        BARNEPENSJON                                   */                
     /* ***************************************************** */                
                                                                                
     IF  B00.PENSJONSTYPE1(W02_I)  = 'B'                                        
       ! B00.PENSJONSTYPE1(W02_I)  = 'N'           THEN                         
       DO;                                                                      
         W01_HJELPE_PTR = ADDR(B00.ETTEBARN(W02_I));                            
         SSA_UQUAL      = 'ETTEBARN ';                                          
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* **************************************************** */         
             /* FEIL VED SKRIV AV   :     ETTEBARN                   */         
             /* **************************************************** */         
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
       END;                                                                     
                                                                                
     /* ***************************************************** */                
     /*        ETTERPENSJON                                   */                
     /* ***************************************************** */                
                                                                                
     IF  B00.PENSJONSTYPE1(W02_I)  = 'E'                                        
       ! B00.PENSJONSTYPE1(W02_I)  = 'J'                                        
       ! B00.PENSJONSTYPE2(W02_I)  = 'E'           THEN                         
       DO;                                                                      
         W01_HJELPE_PTR = ADDR(B00.ETTEPENS(W02_I));                            
         SSA_UQUAL      = 'ETTEPENS ';                                          
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* **************************************************** */         
             /* FEIL VED SKRIV AV   :     ETTEPENS                   */         
             /* **************************************************** */         
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
       END;                                                                     
                                                                                
     /* ***************************************************** */                
     /*        EØS INFO                                       */                
     /* ***************************************************** */                
                                                                                
     IF  B00.EØSINFO.BEREGN_FOLKETRYGD (W02_I)  ^= ' '   THEN                   
       DO;                                                                      
         W01_HJELPE_PTR = ADDR(B00.EØSINFO(W02_I));                             
         SSA_UQUAL      = 'EØSINFO  ';                                          
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* **************************************************** */         
             /* FEIL VED SKRIV AV   :     EØS INFO                   */         
             /* **************************************************** */         
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
       END;                                                                     
                                                                                
     /* ***************************************************** */                
     /*         SPESIALOPPLYSNINGER                           */                
     /* ***************************************************** */                
                                                                                
     IF P_SPESIALOPPLYSNINGER  (B00.SPESOPPL (W02_I)) THEN                      
       DO;                                                                      
         W01_HJELPE_PTR = ADDR (B00.SPESOPPL (W02_I));                          
         SSA_UQUAL      = 'SPESOPPL ';                                          
         CALL P_SKRIV_BARN_AV_STATUS;                                           
                                                                                
         IF  FEIL_MELD_NR              >   0             THEN                   
           DO;                                                                  
             /* **************************************************** */         
             /* FEIL VED SKRIV AV  :   SPESOPPL                      */         
             /* **************************************************** */         
                                                                                
             GO TO RETUR;                                                       
           END;                                                                 
       END;                                                                     
         END;/* IKKE SERVICEBEREGNET AFP-PENSJON */                             
                                                                                
  RETUR:                                                                        
                                                                                
  END P_SKRIV_NEST_SISTE_STATUS;                                                
