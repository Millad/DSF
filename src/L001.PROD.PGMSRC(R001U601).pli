  R001U6:                                                                       
    PROC(COMMAREA_PEKER) OPTIONS(MAIN);                                         
  %PAGE;                                                                        
  %INCLUDE S001U6;    /*  U3-MAPSETTET   */                                     
  %PAGE;                                                                        
  %INCLUDE S00120;    /*  YRKESSKADE-MAPSETTET   */                             
  %PAGE;                                                                        
  %INCLUDE P001U601;  /*  TRANS_OMR FOR UFØREPENSJON       */                   
  %PAGE;                                                                        
  %INCLUDE P0019906;  /*  TRANS_OPPL_OMRÅDE (BASED)             */              
  %PAGE;                                                                        
  %INCLUDE P0019908;  /*  KOM   OMRÅDE (BASED)           */                     
  %PAGE;                                                                        
  %INCLUDE P0019910;  /*  STYRINGS_OMRÅDE (BASED)           */                  
  %PAGE;                                                                        
  %INCLUDE P0019912;  /*  DIV-PARAM_OMRÅDE (BASED)          */                  
  %PAGE;                                                                        
  %INCLUDE DFHBMSCA;                                                            
  %PAGE;                                                                        
                                                                                
    DCL  (BMSMAPBR,COMMAREA_PEKER) PTR;                                         
 /* NULLET UT ETTER KONF. MED PATHAK 10.08.2000 MARTIN */                       
 /* DCL PLIXOPT CHAR(13) VAR STATIC EXTERNAL INIT(                              
                                                'NOSPIE NOSTAE')                
 /* NULLET UT ETTER KONF. MED PATHAK 10.08.2000 MARTIN */                       
   DCL (ADDR, CSTG, LBOUND, LOW, ONCODE, UNSPEC, SUBSTR, VERIFY)                
                                     BUILTIN;                                   
   DCL (I, J, K, N)FIXED BINARY (31) INIT (0);                                  
                                                                                
    DCL                                                                         
       FEIL_FUNNET     BIT (1),                                                 
       FEIL_I_SØKER    BIT (1),                                                 
       FEIL_I_EØS      BIT (1),                                                 
       FEIL_I_SPES     BIT (1),                                                 
       FEIL_I_YSKODE   BIT (1),                                                 
       FEIL_I_BLAD     BIT (1) INIT  ('0'B),                                    
       ONKODE PIC'9999',                                                        
       CURSOR_POS FIXED BIN(15) INIT(-1),                                       
       ONK DEF ONKODE CHAR(4),                                                  
       FEILKODE       CHAR(4),                                                  
       DSNAVN         CHAR(8),                                                  
       ANT_FEIL_SKREVET FIXED DEC (3);                                          
                                                                                
                                                                                
  %PAGE;                                                                        
     /*ON ERROR SNAP BEGIN;                                                     
          ON ERROR SYSTEM        ;                                              
          ONKODE=ONCODE          ;                                              
          FEILKODE  = ONK        ;                                              
          DSNAVN     = EIBDS     ;                                              
          GO TO FEILBEH          ;                                              
       END;*/                                                                   
                                                                                
 FEILKODE  = 'FEIL';                                                            
 DSNAVN     = '        ';                                                       
                                                                                
 EXEC CICS HANDLE CONDITION ERROR(FEILBEH);                                     
 EXEC CICS HANDLE AID PF1(PF1) PF2(PF2) PF7(PF7) PF8(PF8);                      
                                                                                
 KOM_OMR.PEKER_LISTE.DIV_PARAM_PEKER  = ADDR(KOM_OMR.DIV_PARAM_OMR);            
                                                                                
  IF  FEIL_MELD_NR  = 666 !                                                     
     FEIL_MELD_NR = 0 THEN;                                                     
    ELSE                                                                        
                                                                                
        GOTO  FEIL_MELD_TIL_SKJERM;                                             
                                                                                
                                                                                
  /*    GOTO FEILBEH2; */                                                       
                                                                                
 IF FRA_CICS THEN                                                               
  DO;                                                                           
    KOM_OMR.PEKER_LISTE.TRANS_OPPL_PEKER = ADDR(KOM_OMR.TRANS_OPPL_OMR);        
    KOM_OMR.PEKER_LISTE.TRANS_PEKER      = ADDR(KOM_OMR.TRANS_OMR);             
    KOM_OMR.PEKER_LISTE.STYRINGS_PEKER   = ADDR(KOM_OMR.STYRINGS_OMR);          
    ATK_KOM_PTR           = ADDR  (KOM_OMR.ATK_COM_OMR  ) ;                     
  END;                                                                          
                                                                                
 SEND_MAP:                                                                      
                                                                                
 IF HENT_FRAM_MAP  THEN                                                         
   DO;                                                                          
       IF TRANSKODE  = 'RU63'  THEN                                             
          DO;                                                                   
              EXEC CICS SEND MAP('S001U62') MAPSET('S001U63') MAPONLY           
                                                         SET (BMSMAPBR);        
              ALLOCATE S001U62O;                                                
              S001U62O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                   
          END;                                                                  
       ELSE                                                                     
       IF TRANSKODE  = 'RU65'  THEN                                             
          DO;                                                                   
              EXEC CICS SEND MAP('S001U63') MAPSET('S001U63') MAPONLY           
                                                         SET (BMSMAPBR);        
              ALLOCATE S001U63O;                                                
              S001U63O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                   
          END;                                                                  
       ELSE                                                                     
       IF TRANSKODE  = 'RU67'  THEN                                             
          DO;                                                                   
              EXEC CICS SEND MAP('S001201') MAPSET('S001203') MAPONLY           
                                                         SET (BMSMAPBR);        
              ALLOCATE S001201O;                                                
              S001201O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                   
          END;                                                                  
       ELSE                                                                     
          DO;                                                                   
             /* SKJERMBILDE GRUNNBLANKETT-U3 */                                 
             EXEC CICS SEND MAP('S001U61') MAPSET('S001U63') MAPONLY            
                                                         SET (BMSMAPBR);        
             ALLOCATE S001U61O;                                                 
             S001U61O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                    
          END;                                                                  
   END;                                                                         
                                                                                
 RECEIVE_MAP:                                                                   
                                                                                
 IF ^HENT_FRAM_MAP THEN                                                         
    DO;                                                                         
       IF TRANSKODE  = 'RU60'  !                                                
          TRANSKODE  = 'RU61'  THEN                                             
          EXEC CICS RECEIVE MAP('S001U61')                                      
                                    MAPSET('S001U63') SET(BMSMAPBR);            
                                                                                
       ELSE                                                                     
       IF TRANSKODE = 'RU62'  !                                                 
          TRANSKODE = 'RU63'  THEN                                              
          EXEC CICS RECEIVE MAP('S001U62')                                      
                                    MAPSET('S001U63') SET(BMSMAPBR);            
                                                                                
       ELSE                                                                     
       IF TRANSKODE = 'RU64'  !                                                 
          TRANSKODE = 'RU65'  THEN                                              
          EXEC CICS RECEIVE MAP('S001U63')                                      
                                    MAPSET('S001U63') SET(BMSMAPBR);            
                                                                                
       ELSE                                                                     
      IF  TRANSKODE = 'RU66'  !                                                 
          TRANSKODE = 'RU67'  THEN                                              
          EXEC CICS RECEIVE MAP('S001201')                                      
                                    MAPSET('S001203') SET(BMSMAPBR);            
       CALL BLANK_U3_MELDNR;                                                    
       CALL BLANK_U3_SP_MELDNR;                                                 
       CALL BLANK_U3_EØS_MELDNR;                                                
       CALL BLANK_U3_YP_MELDNR;                                                 
       FEIL_MELD_NR = 0;                                                        
    END;                                                                        
                                                                                
 IF TRANSKODE = 'RU67' THEN                                                     
    DO;                                                                         
       IF S001201I.FUNKSJONSKODEL > 0 THEN                                      
          DO;                                                                   
             FUNKSJONSKODE = S001201I.FUNKSJONSKODEI;                           
                                                                                
             /* KONTROLLEN TILBAKE TIL R001030 - VALG AV FUNKSJON */            
             EXEC CICS XCTL PROGRAM('R0010301') COMMAREA(KOM_OMR);              
          END;                                                                  
                                                                                
       IF S001201I.STYRE_KODEL > 0 THEN                                         
          DO;                                                                   
             STYREKODE = S001201I.STYRE_KODEI;                                  
             TRANSKODE = 'R041';                                                
                                                                                
             /* KONTROLLEN TILBAKE TIL R0010401 - REGISTRERING */               
                                                                                
             EXEC CICS XCTL PROGRAM('R0010401') COMMAREA(KOM_OMR);              
          END;                                                                  
    END;                                                                        
 ELSE                                                                           
    DO;                                                                         
       IF S001U61I.FUNKSJONSKODEL > 0 !                                         
          S001U62I.FUNKSJONSKODEL > 0 !                                         
          S001U63I.FUNKSJONSKODEL > 0 THEN                                      
          DO;                                                                   
             IF S001U61I.FUNKSJONSKODEL > 0 THEN                                
                FUNKSJONSKODE = S001U61I.FUNKSJONSKODEI;                        
             IF S001U62I.FUNKSJONSKODEL > 0 THEN                                
                FUNKSJONSKODE = S001U62I.FUNKSJONSKODEI;                        
             IF S001U63I.FUNKSJONSKODEL > 0 THEN                                
                FUNKSJONSKODE = S001U63I.FUNKSJONSKODEI;                        
                                                                                
             /* KONTROLLEN TILBAKE TIL R001030 - VALG AV FUNKSJON */            
             EXEC CICS XCTL PROGRAM('R0010301') COMMAREA(KOM_OMR);              
          END;                                                                  
        ELSE                                                                    
       IF S001U63I.FUNKSJONSKODEL > 0 THEN                                      
          DO;                                                                   
             FUNKSJONSKODE = S001U61I.FUNKSJONSKODEI;                           
                                                                                
             /* KONTROLLEN TILBAKE TIL R001030 - VALG AV FUNKSJON */            
             EXEC CICS XCTL PROGRAM('R0010301') COMMAREA(KOM_OMR);              
          END;                                                                  
       ELSE                                                                     
       IF S001U61I.STYRE_KODEL > 0 !                                            
          S001U62I.STYRE_KODEL > 0 !                                            
          S001U63I.STYRE_KODEL > 0 THEN                                         
          DO;                                                                   
             IF S001U61I.STYRE_KODEL > 0 THEN                                   
                STYREKODE = S001U61I.STYRE_KODEI;                               
             IF S001U62I.STYRE_KODEL > 0 THEN                                   
                STYREKODE = S001U62I.STYRE_KODEI;                               
             IF S001U63I.STYRE_KODEL > 0 THEN                                   
                STYREKODE = S001U63I.STYRE_KODEI;                               
                                                                                
             TRANSKODE = 'R041';                                                
                                                                                
             /* KONTROLLEN TILBAKE TIL R0010401 - REGISTRERING */               
                                                                                
             EXEC CICS XCTL PROGRAM('R0010401') COMMAREA(KOM_OMR);              
          END;                                                                  
    END;                                                                        
                                                                                
                                                                                
 IF STYREKODE ^= 'U3'  &  FRA_CICS THEN                                         
    TRANSKODE = 'R041';                                                         
 ELSE                                                                           
  DO;                                                                           
                                                                                
   TRANS_RETURKODE  = TRANSKODE;                                                
   ANT_FEIL_SKREVET = 0;                                                        
   FEIL_FUNNET      = '0'B;                                                     
   FEIL_I_SØKER     = '0'B;                                                     
   FEIL_I_SPES      = '0'B;                                                     
   FEIL_I_EØS       = '0'B;                                                     
   FEIL_I_YSKODE    = '0'B;                                                     
   FEIL_I_BLAD      = '0'B;                                                     
 IF HENT_FRAM_MAP  THEN                                                         
    DO;                                                                         
       FEIL_I_BLAD      = '1'B;                                                 
       FEIL_I_SØKER     = '1'B;                                                 
    END;                                                                        
   SELECT (TRANSKODE);                                                          
     WHEN('RU60')                                                               
        DO;                                                                     
           CALL BLANK_U3_SØKER;                  /* R001U602 */                 
           CALL BLANK_U3_SPES;                   /* R001U602 */                 
           CALL BLANK_U3_EØS ;                   /* R001U602 */                 
           CALL BLANK_U3_YP;                     /* R001U602 */                 
           CALL BLANK_U3_SP_MELDNR;              /* R001U602 */                 
           CALL BLANK_U3_YP_MELDNR;              /* R001U602 */                 
           CALL BLANK_U3_MELDNR;                 /* R001U602 */                 
           CALL OVERFØR_U3_SØKER;       /* R001U609 FRA MAP TIL OMR */          
           CALL KONTROLL_U3_SØKER(FEIL_I_SØKER); /* R001U603 */                 
        END;                                                                    
     WHEN('RU61')                                                               
        DO;                                                                     
          CALL BLANK_U3_MELDNR;                  /* R001U602 */                 
          CALL OVERFØR_U3_SØKER;        /* R001U609 FRA MAP TIL OMR */          
          CALL KONTROLL_U3_SØKER(FEIL_I_SØKER);  /* R001U603 */                 
        END;                                                                    
     WHEN('RU62')                                                               
        DO;                                                                     
          CALL BLANK_U3_SP_MELDNR;                                              
          IF FUNKSJONSKODE ^= 'V' THEN                                          
          CALL BLANK_U3_SPES;                                                   
                                                                                
          CALL OVERFØR_U3_SPES;          /* R001U609 FRA MAP TIL OMR */         
          CALL KONTROLL_U3_SPES(FEIL_I_SPES);            /* R001U605 */         
                                                                                
        END;                                                                    
     WHEN('RU63')                                                               
        DO;                                                                     
           CALL BLANK_U3_SP_MELDNR;                                             
           CALL OVERFØR_U3_SPES;        /* R001U609 FRA MAP TIL OMR */          
           CALL KONTROLL_U3_SPES(FEIL_I_SPES);           /* R001U605 */         
        END;                                                                    
     WHEN('RU64')                                                               
        DO;                                                                     
          CALL BLANK_U3_EØS_MELDNR;                                             
          IF FUNKSJONSKODE ^= 'V' THEN                                          
          CALL BLANK_U3_EØS ;                                                   
                                                                                
          CALL OVERFØR_U3_EØS  ;          /* R001U609 FRA MAP TIL OMR */        
          CALL KONTROLL_U3_EØS (FEIL_I_EØS);            /* R001U605 */          
                                                                                
        END;                                                                    
     WHEN('RU65')                                                               
        DO;                                                                     
           CALL BLANK_U3_EØS_MELDNR;                                            
           CALL OVERFØR_U3_EØS;        /* R001U609 FRA MAP TIL OMR */           
           CALL KONTROLL_U3_EØS (FEIL_I_EØS);           /* R001U605 */          
        END;                                                                    
     WHEN('RU66')                                                               
        DO;                                                                     
           CALL BLANK_U3_YP_MELDNR;                                             
          IF FUNKSJONSKODE ^= 'V' THEN                                          
           CALL BLANK_U3_YP;                             /* R001U602 */         
           CALL OVERFØR_U3_YP;           /* R001U609 FRA MAP TIL OMR */         
           CALL KONTROLL_U3_YP  (FEIL_I_YSKODE);      /* R001U605 */            
        END;                                                                    
     WHEN('RU67')                                                               
        DO;                                                                     
                                                                                
           CALL BLANK_U3_YP_MELDNR;                                             
           CALL OVERFØR_U3_YP;                                                  
           CALL KONTROLL_U3_YP(FEIL_I_YSKODE);                                  
        END;                                                                    
     OTHERWISE;                                                                 
        /*INGEN UTGANG DA CICS IKKE GODTAR UGYLDIGE TRANSAKSJONSKODER */        
  END;                                                                          
                                                                                
                                                                                
     IF FEIL_I_SØKER   = '0'B &                                                 
        FEIL_I_SPES    = '0'B &                                                 
        FEIL_I_EØS     = '0'B &                                                 
        FEIL_I_YSKODE  = '0'B THEN                                              
        FEIL_FUNNET    = '0'B;                                                  
     ELSE                                                                       
           FEIL_FUNNET = '1'B;                                                  
                                                                                
  IF ^FEIL_FUNNET   THEN                                                        
      DO;                                                                       
         IF (TRANSKODE = 'RU60' !                                               
             TRANSKODE = 'RU61'  )    &                                         
             U3S.NORSK_BOSATT   = 'J' &                                         
            (U3S.YSKADE_TILLEGG = ' ' !                                         
             U3S.YSKADE_TILLEGG = 'N' ) THEN                                    
              TRANSKODE = 'RU61';                                               
                                                                                
         ELSE                                                                   
         IF (TRANSKODE = 'RU60' !                                               
             TRANSKODE = 'RU61'  )    &                                         
            (U3S.NORSK_BOSATT   = 'J' &                                         
             U3S.YSKADE_TILLEGG = 'J' ) THEN                                    
              TRANSKODE = 'RU66';                                               
                                                                                
         ELSE                                                                   
         IF (TRANSKODE = 'RU60' !                                               
             TRANSKODE = 'RU61'  )    &                                         
             U3S.NORSK_BOSATT   = 'N'   THEN                                    
              TRANSKODE = 'RU62';                                               
                                                                                
         ELSE                                                                   
         IF (TRANSKODE = 'RU62' !                                               
             TRANSKODE = 'RU63' )  &                                            
             U3SP.EØS_OPPLYSN    = 'J'   THEN                                   
              TRANSKODE = 'RU64';                                               
                                                                                
         ELSE                                                                   
         IF (TRANSKODE = 'RU62' !                                               
             TRANSKODE = 'RU63' !                                               
             TRANSKODE = 'RU64' !                                               
             TRANSKODE = 'RU65'  )&                                             
             U3S.YSKADE_TILLEGG = 'J'   THEN                                    
              TRANSKODE = 'RU66';                                               
                                                                                
         ELSE                                                                   
         IF (TRANSKODE = 'RU62' !                                               
             TRANSKODE = 'RU63' )  &                                            
            (U3SP.EØS_OPPLYSN    = ' ' !                                        
             U3SP.EØS_OPPLYSN    = 'N' ) THEN                                   
              TRANSKODE = 'RU63';                                               
                                                                                
         ELSE                                                                   
         IF  TRANSKODE = 'RU62'  &                                              
             (U3S.YSKADE_TILLEGG = ' ' !                                        
              U3S.YSKADE_TILLEGG = 'N'   )   THEN                               
              TRANSKODE = 'RU63';                                               
         ELSE                                                                   
         IF  TRANSKODE = 'RU64' !                                               
             TRANSKODE = 'RU65'    THEN                                         
              TRANSKODE = 'RU65';                                               
                                                                                
         ELSE                                                                   
         IF  TRANSKODE = 'RU66' !                                               
             TRANSKODE = 'RU67'    THEN                                         
              TRANSKODE = 'RU67';                                               
                                                                                
         TRANS_RETURKODE  = TRANSKODE ;                                         
      END;                                                                      
                                                                                
                                                                                
                                                                                
  IF FEIL_FUNNET ! HENT_FRAM_MAP !                                              
     FRA_UTEN_DIALOG ! TRANS_RETURKODE ^= TRANSKODE THEN                        
     DO;                                                                        
         IF TRANSKODE = 'RU60' THEN                                             
            TRANSKODE = 'RU61';                                                 
                                                                                
