              END;                                                              
           OTHERWISE;                                                           
        END;                                                                    
                                                                                
      IF HENT_FRAM_MAP THEN                                                     
         DO;                                                                    
           IF TRANSKODE = 'RU60' !                                              
              TRANSKODE = 'RU61'   THEN                                         
              DO;                                                               
                 EXEC CICS SEND MAP('S001U61') MAPSET('S001U63')                
                                        ERASE CURSOR;                           
              END;                                                              
           ELSE                                                                 
           IF TRANSKODE = 'RU62' !                                              
              TRANSKODE = 'RU63' THEN                                           
              DO;                                                               
                 EXEC CICS SEND MAP('S001U62') MAPSET('S001U63')                
                                        ERASE CURSOR;                           
             END;                                                               
           ELSE                                                                 
           IF TRANSKODE = 'RU64' !                                              
              TRANSKODE = 'RU65' THEN                                           
              DO;                                                               
                 EXEC CICS SEND MAP('S001U63') MAPSET('S001U63')                
                                        ERASE CURSOR;                           
             END;                                                               
           ELSE                                                                 
           IF TRANSKODE = 'RU66' !                                              
              TRANSKODE = 'RU67' THEN                                           
              EXEC CICS SEND MAP('S001201') MAPSET('S001203') ERASE             
                                                                CURSOR;         
         END;                                                                   
      ELSE                                                                      
         DO;                                                                    
           IF TRANSKODE = 'RU60' !                                              
              TRANSKODE = 'RU61'   THEN                                         
              EXEC CICS SEND MAP('S001U61') MAPSET('S001U63') ERASEAUP          
                                                     DATAONLY CURSOR;           
           ELSE                                                                 
           IF TRANSKODE = 'RU62' !                                              
              TRANSKODE = 'RU63' THEN                                           
              EXEC CICS SEND MAP('S001U62') MAPSET('S001U63') ERASEAUP          
                                                     DATAONLY CURSOR;           
           ELSE                                                                 
           IF TRANSKODE = 'RU64' !                                              
              TRANSKODE = 'RU65' THEN                                           
              EXEC CICS SEND MAP('S001U63') MAPSET('S001U63') ERASEAUP          
                                                     DATAONLY CURSOR;           
           ELSE                                                                 
           IF TRANSKODE = 'RU66' !                                              
              TRANSKODE = 'RU67' THEN                                           
              EXEC CICS SEND MAP('S001201') MAPSET('S001203') ERASEAUP          
                                                       DATAONLY CURSOR;         
         END;                                                                   
      HENT_FRAM_MAP = '0'B;                                                     
                                                                                
                                                                                
                                                                                
      FEIL_MELD_NR  = 666; /*  => FEIL_FUNNET ER SATT. */                       
                                                                                
      IF  FRA_CICS THEN                                                         
        EXEC CICS RETURN TRANSID(TRANSKODE) COMMAREA(KOM_OMR);                  
      ELSE                                                                      
        GOTO RECEIVE_MAP;                                                       
    END;                                                                        
                                                                                
    ELSE                                                                        
       DO;                                                                      
                                                                                
           IF TRANSKODE = 'RU62' THEN                                           
              DO;                                                               
                ALLOCATE S001U62O;                                              
                S001U62O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                 
                S001U62I.STATSBORGERL = CURSOR_POS;                             
                                                                                
                /*----------------------------------------------------*/        
                /* OVERFØRER DATA TIL MAP HVIS VENTETRANS-BEHANDLING  */        
                /*----------------------------------------------------*/        
                                                                                
                IF FUNKSJONSKODE     = 'V'    THEN                              
                      CALL OVERFØR_U3_SP_MAP;                                   
                                                                                
                      EXEC CICS SEND MAP('S001U62') MAPSET('S001U63')           
                                        ERASE CURSOR;                           
                                                                                
                                                                                
                HENT_FRAM_MAP = '0'B;                                           
                                                                                
                FEIL_MELD_NR  = 666;      /* => FEIL_FUNNET ER SATT. */         
                                                                                
                IF FRA_CICS THEN                                                
                   EXEC CICS RETURN TRANSID(TRANSKODE)                          
                                    COMMAREA(KOM_OMR);                          
                ELSE                                                            
                   GOTO RECEIVE_MAP;                                            
              END;                                                              
           IF TRANSKODE = 'RU64' THEN                                           
              DO;                                                               
                ALLOCATE S001U63O;                                              
                S001U63O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                 
                S001U63I.BEREGN_FOLKETRYGDL = CURSOR_POS;                       
                                                                                
                /*----------------------------------------------------*/        
                /* OVERFØRER DATA TIL MAP HVIS VENTETRANS-BEHANDLING  */        
                /*----------------------------------------------------*/        
                                                                                
                IF FUNKSJONSKODE     = 'V'    THEN                              
                      CALL OVERFØR_U3_EØS_MAP;                                  
                                                                                
                      EXEC CICS SEND MAP('S001U63') MAPSET('S001U63')           
                                        ERASE CURSOR;                           
                                                                                
                                                                                
                HENT_FRAM_MAP = '0'B;                                           
                                                                                
                FEIL_MELD_NR  = 666;      /* => FEIL_FUNNET ER SATT. */         
                                                                                
                IF FRA_CICS THEN                                                
                   EXEC CICS RETURN TRANSID(TRANSKODE)                          
                                    COMMAREA(KOM_OMR);                          
                ELSE                                                            
                   GOTO RECEIVE_MAP;                                            
              END;                                                              
                                                                                
          IF TRANSKODE          = 'RU66'  &                                     
             U3S.YSKADE_TILLEGG  = 'J'     THEN                                 
             DO;                                                                
                                                                                
                ALLOCATE S001201O;                                              
                S001201O.CICS_INFOO = DIV_PARAM_OMR.CICS_NAVN ;                 
                S001201O.FNRO  = F_FELT_PIC_CHAR11(U3S.FNR);                    
                S001201O.TKNRO = F_FELT_PIC_CHAR4(U3S.TKNR);                    
                S001201O.NAVNO = U3S.NAVN;                                      
                S001201O.VIRK_DATOO =               /* 2000-3 */                
                                   KONV_HÅMD_MÅ(U3S.VIRK_DATO_ÅMD);             
                                          /* 2000-3 */                          
                S001201O.UFTO = KONV_HÅMD_MÅ(U3S.UFT_ÅMD);                      
                                                                                
                /*----------------------------------------------------*/        
                /* OVERFØRER DATA TIL MAP HVIS VENTETRANS-BEHANDLING  */        
                /*----------------------------------------------------*/        
                                                                                
                IF FUNKSJONSKODE     = 'V'    THEN                              
                   CALL OVERFØR_U3_YP_MAP;                                      
                                                                                
                EXEC CICS SEND MAP('S001201') MAPSET('S001203')                 
                                        ERASE CURSOR;                           
            /*  TRANSKODE     = 'RU65';   */                                    
                                                                                
                HENT_FRAM_MAP = '0'B;                                           
                                                                                
                FEIL_MELD_NR  = 666;/* => FEIL_FUNNET ER SATT. */               
                                                                                
                IF FRA_CICS THEN                                                
                   EXEC CICS RETURN TRANSID(TRANSKODE)                          
                                    COMMAREA(KOM_OMR);                          
                ELSE                                                            
                   GOTO RECEIVE_MAP;                                            
             END;                                                               
                                                                                
          IF TRANSKODE = 'RU60' THEN                                            
             TRANS_RETURKODE = 'RU61';                                          
          ELSE                                                                  
          IF TRANSKODE = 'RU62' THEN                                            
             TRANS_RETURKODE = 'RU63';                                          
          ELSE                                                                  
          IF TRANSKODE = 'RU64' THEN                                            
             TRANS_RETURKODE = 'RU65';                                          
          ELSE                                                                  
             TRANS_RETURKODE = TRANSKODE;                                       
          IF FRA_CICS  THEN                                                     
             DO;                                                                
                BLANKETTYPE = STYREKODE;                                        
                TRANSKODE = 'R042';                                             
             END;                                                               
       END;                                                                     
   END;                                                                         
                                                                                
