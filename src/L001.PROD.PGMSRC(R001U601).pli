           WHEN ('RU61')                                                        
              DO;                                                               
                 CALL BLANK_U3_SØKER_MAP;              /* R001U609 */           
                 CALL OVERFØR_U3_SØKER_MAP;  /* R001U606 OMR TIL MAP */         
